 retrieve="  SELECT B.STYLE,   B.CHNO,   
         B.MAT_CD,   B.MAT_NM,   
         B.CUST_CD,   dbo.sf_cust_nm(B.CUST_CD,'s') AS CUST_NM, 
         B.MAKE_PRICE,   B.ETC_PRICE,   
         B.REQ_YMD,   B.ORD_YMD,   B.TAG_YMD,   
         B.FIX_DLVY,   B.DLVY_YMD,   B.LAST_YMD,   
         ISNULL(B.IWOL_MARK, '0') AS IWOL_MARK,   
         B.IWOL_YMD,   B.REMARK,   
         B.REG_ID,   B.REG_DT,   B.MOD_ID,   B.MOD_DT,   
         A.BRAND,   dbo.sf_inter_nm('001', A.BRAND, :as_lang) AS BRAND_NM,
         A.YEAR,   
         A.SEASON,   dbo.sf_inter_nm('003', A.SEASON, :as_lang) AS SEASON_NM,
         A.SOJAE,   dbo.sf_sojae_nm(A.SOJAE, :as_lang) AS SOJAE_NM,
         A.ITEM,   dbo.sf_item_nm(A.ITEM, :as_lang) AS ITEM_NM,
         A.OUT_SEQ,   dbo.sf_inter_nm('010', A.OUT_SEQ, :as_lang) AS OUT_SEQ_NM, 
         A.SAMPLE_CD,   
         isnull(b.make_type,A.MAKE_TYPE) make_type,   dbo.sf_inter_nm('030', isnull(b.make_type,A.MAKE_TYPE), :as_lang) AS MAKE_TYPE_NM, 
         A.PATT_TYPE,   dbo.sf_inter_nm('020', A.PATT_TYPE, :as_lang) AS PATT_TYPE_NM, 
         isnull(b.orgmat_cd,A.ORGMAT_CD) orgmat_cd,   dbo.sf_inter_nm('021', isnull(b.orgmat_cd,A.ORGMAT_CD), :as_lang) AS ORGMAT_NM, 
         A.DSGN_EMP,   dbo.sf_emp_nm(A.DSGN_EMP)   AS DSGN_EMP_NM, 
         A.CONCEPT,   dbo.sf_inter_nm('122', A.CONCEPT, :as_lang) AS CONCEPT_NM, 
         A.PLAN_YN,   
         isnull(b.country_cd,A.COUNTRY_CD) country_cd,   dbo.sf_inter_nm('000', isnull(b.country_cd, isnull(A.COUNTRY_CD,'00')), :as_lang) AS COUNTRY_NM, 
         A.TAG_PRICE,   
         A.STYLE_INFO ,
		   (select min(in_ymd) from tb_12030_s where style = b.style and chno = b.chno) fin_ymd,
			b.cancel_yn,
			b.pay_gubn,
			case when exists (select top 1 * from tb_41010_h (nolock)
			where style = b.style
			and   chno  = b.chno
			and   bill_date is not null) then 1 else 0 end  as make_price_protect,
			(select max(isnull(note, dbo.sf_inter_nm('211',claim_gubn, :as_lang) )) from tb_23010_m (nolock) where style = b.style and chno = b.chno) 	claim_note,
			a.smat_info,
			b.zsojae, b.zitem
    FROM TB_12020_M  A(nolock),   
         TB_12021_D  B(nolock) 
   WHERE B.STYLE  = A.STYLE 
     AND B.CHNO  = :as_chno
     AND (A.STYLE = :as_style) 
" update="TB_12021_D" updatewhere=0 updatekeyinplace=yes arguments=(("as_style", string),("as_chno", string),("as_lang", string)) )
