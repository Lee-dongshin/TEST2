 column=(type=decimal(2) updatewhereclause=yes name=dc_rate dbname="tb_56012_d.DC_RATE" )
 retrieve="

SELECT 	STYLE, 
	DBO.SF_FIRST_PRICE(STYLE) AS TAG_PRICE, 
	START_YMD, END_YMD, 
	SALE_TYPE + ' ' + DBO.SF_INTER_NM( '011',SALE_TYPE) AS SALE_TYPE	, 
	CASE WHEN DC_RATE <> 0 THEN 
		DBO.SF_FIRST_PRICE(STYLE)  * (100 - DC_RATE) / 100
	ELSE	sale_price
	END AS SALE_PRICE,
	DC_RATE
FROM TB_56012_D
WHERE SHOP_CD = :AS_SHOP_CD
AND STYLE  = :AS_STYLE
" arguments=(("AS_SHOP_CD", string),("AS_STYLE", string)) )
