 retrieve="

select 	yymmdd,
			shop_cd,
			shop_type,
			sale_no,
			empno,		
			pay_way,
			sale_amt,
			card_amt,
			cash_amt,
			card_gubn,
			reg_id,
			reg_dt,
			mod_id,
			mod_dt,
			'      '		aS empno1,
		   case when empno is null then null 
			else dbo.sf_emp_nm(empno) end as emp_nm,
			'      '		aS cust_cd1,
			cust_cd,
		   case when cust_cd is null then null 
			else dbo.sf_cust_nm(cust_cd,'s') end as cust_nm
from  tb_53011_h (nolock)
where yymmdd    = :as_yymmdd
  and shop_cd   in ('0X3300','1X3300','2X3300','3X3300','4X3300','5X3300', :as_shop_cd)
  and shop_type = :as_shop_type
  and sale_no   = :as_sale_no
  and reg_id    = :As_shop_cd
" update="tb_53011_h" updatewhere=1 updatekeyinplace=no arguments=(("as_yymmdd", string),("as_shop_cd", string),("as_shop_type", string),("as_sale_no", string)) )
