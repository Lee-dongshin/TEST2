 retrieve="  SELECT jumin,   
         user_name,   
         card_day, 
         card_no,   
         right(card_no, 9)  as card_no2, 
			birthday,
			bday,
         shop_cd,   
         area,   
         sex,
			Case post_flag when 0 then zipcode else zipcode2 end as zipcode_v,
			zipcode,
			zipcode2,
			post_flag,
			Case post_flag when 0 then addr else addr2 end as addrv,
			Case post_flag when 0 then addr_s else addr2_s end as addrv_s,
			addr,
			addr_s,
			addr2,
			addr2_s,
			email,
			'                    '     as email1,
			'                    '     as email2,
			tel_no3,
			'    '     as tel_no3a,
			'    '     as tel_no3b,
			'    '     as tel_no3c,
			tel_no1,
			'    '     as tel_no1a,
			'    '     as tel_no1b,
			'    '     as tel_no1C,
         Reg_id,   
         Reg_Dt,   
         Mod_id,   
         Mod_Dt,
			CASE when Exists (select top 1 jumin from beaucre.dbo.tb_71010_m with (nolock)
			where jumin = a.jumin
			and isnull(card_no,'') <> '') then 1 else 0 end as prot,
			job,
			tm_yn,
			dm_yn,
			mailing,
			sms_yn,
			recommender,
			(select user_name 
			 from beaucre.dbo.TB_71010_M (nolock)
			 where card_no = a.recommender ) as rec_user_name,
			(select tel_no3 
			 from beaucre.dbo.TB_71010_M (nolock)
			 where card_no = a.recommender )		as rec_tel_no
   FROM beaucre.dbo.TB_71010_M a
   WHERE jumin = :as_jumin " update="beaucre.dbo.TB_71010_M" updatewhere=0 updatekeyinplace=yes arguments=(("as_jumin", string)) )
