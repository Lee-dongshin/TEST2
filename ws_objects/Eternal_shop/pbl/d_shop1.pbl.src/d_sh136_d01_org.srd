 column=(type=decimal(0) updatewhereclause=yes name=deal_qty dbname="deal_qty" )
 retrieve="select 	out_ymd, style, 
	color + ' ' + dbo.sf_color_nm(color,'k') as color, 
	size, 
	dbo.sf_first_price(style)		as tag_price,
	sum(deal_qty)	as deal_qty
from tb_52031_h (nolock)
where out_ymd between :as_fr_ymd and :as_to_ymd
and shop_cd = :as_shop_cd
and deal_fg = '2'
and deal_seq = case when left(style,1) = 'N' then case when out_ymd = '20101122' then 2 else 9 end
		    when left(style,1) = 'O' then 51
		    else 109 end
group by out_ymd, style,color,size
having sum(deal_qty) <> 0" arguments=(("as_fr_ymd", string),("as_to_ymd", string),("as_shop_cd", string)) )
