 retrieve="
select 

upper(A.IBUserId) as IBUserId,	
B.KNAME,
left(A.IBWDate,8)	AS RQST_YMD,	
left(A.IBUDate,8)	AS PROC_YMD,	
A.IBUserDef_1	   	AS RQST_CONT,
A.IBUserDef_5			AS RPOC_GUBN,
A.IBUserDef_7			AS DEPT_NAME
from WORKCREWNET.DBO.IBDocs_0000000001 A (NOLOCK) , MIS.DBO.THB01 B (NOLOCK)
WHERE left(A.IBWDate,8) BETWEEN :AS_FR_YMD AND :aS_TO_YMD
AND   A.IBUserDef_5 LIKE :AS_PROC_GUBN + '%'
AND   A.IBUserId = B.EMPNO
" arguments=(("AS_FR_YMD", string),("AS_TO_YMD", string),("AS_PROC_GUBN", string)) )
