 retrieve="  SELECT A.SHOP_CD,		A.SHOP_NM,		A.SHOP_SNM,   
         A.BRAND,			A.SHOP_DIV,		A.SHOP_SEQ,   
         A.CUST_CD,		A.OWNER_NM,		A.PHONE_NO,
			A.TEL_NO,		A.FAX_NO,      A.DAE_NO, 		A.KYOUNG_NO,
			A.EMPNO,			dbo.sf_emp_nm(A.EMPNO) AS EMP_NM,
			A.SHOP_GRP,		A.AREA_CD,		A.SHOP_CLASS,
			A.POS_YN,		A.SHOP_STAT,
         A.OPEN_YMD,		A.CLOSE_YMD,	
			A.SUGM_MM1,		A.SUGM_DD1,		A.SUGM_RATE1,	
			A.SUGM_MM2,		A.SUGM_DD2,		A.SUGM_RATE2,
			A.SLIP_DD,		A.VAT_WAY,		A.AMT_FG,
         A.BANK_CD,
			A.REG_ID,		A.REG_DT,
			A.MOD_ID,		A.MOD_DT,
			B.CUST_NAME,	B.CUST_SNAME,	B.SAUP_NO,
			B.UPTE,			B.JONGMOK,
			B.CUST_ZIP,		B.CUST_ADDR,
			B.OWNR_IDNO,	
         CONVERT(VARCHAR(8), CAST(B.CHANGE_DATE AS DATETIME), 112) AS CHANGE_DATE,
         B.REMARK,
			a.card_auto_make,
			a.sugm_ymd,
			a.edi_code,
			a.edi_opt,
			a.pay_way,
			A.ETC_SHOP_DIV,
			a.icube_cust,
			a.shop_channel,
			a.shop_major_class,
			a.shop_mid_class,
			a.shop_diff_type,
			a.shop_knm,
			a.shop_div_fin

    FROM TB_91100_M A,
			VI_91102_1 B
	WHERE A.CUST_CD *= B.CUSTCODE
	  AND A.SHOP_CD = :as_shop_cd
" update="TB_91100_M" updatewhere=0 updatekeyinplace=no arguments=(("as_shop_cd", string)) )
