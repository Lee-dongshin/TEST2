 retrieve="  SELECT PERSON_ID, 
         PERSON_NM, 
         PASS_WD, 
         USER_LEVEL, 
         USER_GRP, 
         BRAND, 
         STATUS_YN, 
			IWOL_FR_YMD,
			IWOL_TO_YMD,
			ST_YMD,
			ED_YMD,
         REG_ID, 
         REG_DT, 
         MOD_ID, 
         MOD_DT,
			work_brand,
			work_type,
			work_sojae,
			work_level,
			data_level 
    FROM TB_93010_M (nolock)
   WHERE PERSON_ID LIKE :as_user_id + '%' 
	  and isnull(user_grp,'') like :as_user_grp + '%'
     AND ( CASE PERSON_NM WHEN NULL THEN ' ' ELSE PERSON_NM END ) LIKE :as_user_nm + '%' 
 " update="TB_93010_M" updatewhere=1 updatekeyinplace=no arguments=(("as_user_id", string),("as_user_nm", string),("as_user_grp", string))  sort="user_grp_nm A person_id A " )
