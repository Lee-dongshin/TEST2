 retrieve="  SELECT YYMMDD, SEQ_NO, NO, 
         STYLE, CHNO, COLOR, SIZE,
         SALE_YMD, RCV_SPOT, 
         WASH_FG, WASH_NO, HAN_MARK, MIX_MARK,
         RCV_WHY, RCV_REQ, PROBLEM, REQUEST,
         JUDG_YMD, 
         ISNULL(JUDG_FG, '0') AS JUDG_FG, dbo.sf_inter_nm('799', ISNULL(JUDG_FG, '0')) AS JUDG_FG_NM,
         JUDG_L, JUDG_S, RESULT,
         DEPT_CD, 
         ISNULL(JUDG_CUST_FG, '0') AS JUDG_CUST_FG,
         CUST_CD,     dbo.sf_cust_nm(CUST_CD, 'S')     AS CUST_NM,
         CUST_EMP,   
         ST_CUST_CD,  dbo.sf_cust_nm(ST_CUST_CD, 'S')  AS ST_CUST_NM,
         MAT_CUST_CD, dbo.sf_cust_nm(MAT_CUST_CD, 'S') AS MAT_CUST_NM,
         MAT_CD,      dbo.sf_mat_nm (MAT_CD) AS MAT_NM,
         PAY_FG, 
			PAY_AMT,
			DEAL_FG,
         ST_YMD, PR_YMD, ED_YMD, GO_YMD,
         REMARK, BRAND, YEAR, SEASON, SOJAE, ITEM,
         REG_ID, REG_DT, MOD_ID, MOD_DT,
         STYLE + CHNO AS STYLE_NO,
         dbo.sf_first_price(STYLE) AS TAG_PRICE,
         'N' AS PRINT_YN,
			CLAIM_FG,	PAY_YN,
			CUST_FG_L, CUST_FG_S,
			decision_a, decision_b,
			decision_c, decision_d,
			rtrn_ymd,	rtrn_no,
			ded_ymd, fix_cust,
			dbo.sf_cust_nm(fix_cust, 'S')  AS fix_CUST_sNM,
			fix_cust_nm
    FROM TB_79011_D with (nolock)
   WHERE YYMMDD = :as_yymmdd
     AND SEQ_NO = :as_seq_no
" update="TB_79011_D" updatewhere=0 updatekeyinplace=no arguments=(("as_yymmdd", string),("as_seq_no", string)) )
