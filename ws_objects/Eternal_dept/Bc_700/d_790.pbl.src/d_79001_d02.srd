 retrieve="  SELECT b.YYMMDD	as yymmdd, 
	 b.SEQ_NO	as seq_no, 
         b.JUMIN	as jumin, 
	 b.CARD_NO	as card_no, 
	 b.CUSTOM_NM	as custom_nm,
         b.SEX		as sex, 
	 b.TEL_NO1	as tel_no1, 
	 b.TEL_NO2	as tel_no2, 
	 b.TEL_NO3	as tel_no3, 
         b.SHOP_CD	as shop_cd, 
	 dbo.sf_shop_nm(b.SHOP_CD, 'S') AS SHOP_NM, 
	 b.RCV_HOW	as rcv_how, 
         b.WANT_YMD	as want_ymd, 
	 b.BRAND	as brand, 
	 b.REG_ID	as reg_id, 
	 b.REG_DT	as reg_dt, 
	 b.MOD_ID	as mod_id, 
	 b.MOD_DT	as mod_dt, 
	 b.appoint_ymd,
	(select a.tel_no
         from tb_91100_m a with (nolock)
         where a.shop_cd = b.shop_cd) as tel_no,
	receipt_ymd,
	isnull(confirm_yn,'N') as confirm_yn, 
	isnull(receipt_type,'N') as receipt_type,
	receipt_empno	
    FROM TB_79010_M b with (nolock)
   WHERE b.YYMMDD = :as_yymmdd
     AND b.SEQ_NO = :as_seq_no
 " update="TB_79010_M" updatewhere=0 updatekeyinplace=yes arguments=(("as_yymmdd", string),("as_seq_no", string)) )
