 retrieve="select 	a.YYMMDD,
	a.SEQ_NO,
	a.STYLE,
	a.CHNO,
	a.DSGN_EMP,
   dbo.sf_emp_nm(a.DSGN_EMP) as dsgn_nm,
	a.RECEIPT_EX,
	a.DEPT_OPINION,
	a.HOW_HANDLE,
	a.BRAND,
	a.YEAR,
	a.SEASON,
	a.REG_ID,
	a.REG_DT,
	a.MOD_ID,
	a.MOD_DT,
	(select b.cust_cd
    from vi_12020_1 b
	 where a.style = b.style
      and a.chno = b.chno) AS CUST_CD,
	DBO.SF_CUST_NM((select b.cust_cd
    from vi_12020_1 b
	 where a.style = b.style
      and a.chno = b.chno),'S') AS CUST_NM,

	(select SUM(ISNULL(B.ORD_QTY,0))
    from TB_12030_S B (NOLOCK)
	 where a.style = b.style
      and a.chno = b.chno) AS ORD_QTY,

	(select SUM(ISNULL(B.SALE_QTY,0))
    from TB_12030_S B (NOLOCK)
	 where a.style = b.style
      and a.chno = b.chno) AS SALE_QTY,

   (SELECT COUNT(B.SEQ_NO)
    FROM VI_79010_1 B (NOLOCK)
    WHERE A.STYLE = B.STYLE
      AND A.CHNO  = B.CHNO
	   and a.receipt_type = isnull(a.receipt_type, :As_receipt_type)
   	 ) AS RECEIPT_QTY,
	'\\220.118.68.4\photo\claim_ex\' + A.BRAND + A.YYMMDD + A.SEQ_NO  + '.jpg'		AS SERIAL,
	~"                                                                                                                                    ~"		as file_nm,
	isnull(a.receipt_type, :As_receipt_type) 		as receipt_type
	

from tb_79012_h  a (nolock)
where a.yymmdd = :As_yymmdd
and   a.seq_no = :as_seq_no
and   a.brand  = :as_brand

" update="tb_79012_h" updatewhere=1 updatekeyinplace=no arguments=(("as_yymmdd", string),("as_seq_no", string),("as_brand", string),("as_receipt_type", string)) )
