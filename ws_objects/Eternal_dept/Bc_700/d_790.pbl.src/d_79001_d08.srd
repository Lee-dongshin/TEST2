 column=(type=char(12) updatewhereclause=yes name=amt dbname="amt" )
 retrieve="select	'' as cbit,
			a.yymmdd,
			a.seq_no,
			a.custom_nm,
			a.shop_cd,
			isnull(dbo.sf_shop_nm(a.SHOP_CD, 'S'),'') AS shop_nm,
			b.style,
			b.problem,
			case	when isnull(b.pay_fg,'') = ''  then '무상'
					when isnull(b.pay_fg,'') = '1' then '무상'
					when isnull(b.pay_fg,'') = '2' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '3' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '4' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '5' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '5' and isnull(b.pay_amt,0) > 0 then convert(varchar(12),b.pay_amt)
					when isnull(b.pay_fg,'') = '6' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '6' and isnull(b.pay_amt,0) > 0 then convert(varchar(12),b.pay_kamt)
			else '' end as amt
from		tb_79010_m a with (nolock) inner join tb_79011_d b with (nolock) on a.yymmdd = b.yymmdd and a.seq_no = b.seq_no
WHERE		a.YYMMDD BETWEEN :as_fr_ymd AND :as_to_ymd
    		AND ISNULL(a.JUMIN, '%')     LIKE    :as_jumin
			AND ISNULL(a.CARD_NO, '%')   LIKE    :as_card_no
			AND ISNULL(a.CUSTOM_NM, '%') LIKE    :as_custom_nm
			AND a.BRAND     =       :as_brand
			AND ISNULL(a.SHOP_CD, '%')   LIKE    :as_shop_cd
			AND isnull(a.receipt_ymd,'0') between isnull(:rct_fr_ymd,'0') and isnull(:rct_to_ymd,'9')
" arguments=(("as_brand", string),("as_fr_ymd", string),("as_to_ymd", string),("as_shop_cd", string),("as_card_no", string),("as_jumin", string),("as_custom_nm", string),("rct_fr_ymd", string),("rct_to_ymd", string)) )
