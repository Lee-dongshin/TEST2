 retrieve="  SELECT ITEM, dbo.sf_item_nm(ITEM) AS ITEM_NM,
         COUNT(YYMMDD) AS QTY
    FROM TB_79011_D with (nolock)
   WHERE YYMMDD BETWEEN :as_fr_ymd AND :as_to_ymd
     AND ISNULL(DEPT_CD, '%') LIKE :as_dept_cd
     AND BRAND = :as_brand
     AND ISNULL(JUDG_FG, '%') LIKE :as_judg_fg
     AND ISNULL(DEAL_FG, '%') LIKE :as_deal_fg
   GROUP BY ITEM
" arguments=(("as_brand", string),("as_fr_ymd", string),("as_to_ymd", string),("as_dept_cd", string),("as_judg_fg", string),("as_deal_fg", string))  sort="item A " )
