 retrieve="  SELECT A.BRAND,			A.IN_YMD,		A.IN_GUBN,		A.IN_NO,   
         A.IO_GUBN,		A.HOUSE,			A.CUST_CD,		A.MAT_CD,   
         A.COLOR,			dbo.sf_color_nm(A.COLOR, 'E') AS COLOR_NM,
         A.SPEC,			A.UNIT,			A.H_LINE,		A.V_LINE,   
         ISNULL(A.TMP_QTY, 0) AS TMP_QTY, 
         0.00 AS QC_QTY, 
         ISNULL(A.DFT_QTY, 0) AS DFT_QTY, 
         ISNULL(A.SIL_QTY, 0) AS SIL_QTY, 
         ISNULL(A.PRICE,   0) AS PRICE, 
         ISNULL(A.AMT,     0) AS AMT,
         A.ORD_YMD,		A.ORD_NO,		A.ORD_ORIGIN,	
         A.MAT_YEAR,		A.MAT_SEASON,	A.MAT_SOJAE,
         A.MAT_CHNO,		A.OUT_SEQ,		A.MAT_GUBN,
			A.REG_ID
    FROM TB_22010_H A
   WHERE A.BRAND   = :as_brand
     AND A.IN_GUBN = '01'
     AND A.HOUSE   = :as_house
     AND A.MAT_CD  = :as_mmat_cd
     AND A.IO_GUBN = '-'
" update="TB_22010_H" updatewhere=0 updatekeyinplace=no arguments=(("as_brand", string),("as_mmat_cd", string),("as_house", string))  sort="in_ymd A in_no A " )
