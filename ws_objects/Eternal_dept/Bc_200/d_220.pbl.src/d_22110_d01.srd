 retrieve="  SELECT A.BRAND, A.OUT_YMD, A.OUT_GUBN, A.OUT_NO,
         A.IO_GUBN, A.HOUSE, A.CUST_CD, 
         A.MAT_CD, A.COLOR, A.COLOR AS COLOR1, A.COLOR AS COLOR2,
         A.SPEC, A.UNIT, 
         ISNULL(A.QTY, 0) AS QTY, 
         ISNULL(A.PRICE, 0) AS PRICE, 
         ISNULL(A.AMT, 0) AS AMT, 
         A.MAT_YEAR, A.MAT_SEASON, A.MAT_SOJAE, A.MAT_CHNO, A.OUT_SEQ, A.MAT_GUBN,
         A.REG_ID, A.REG_DT, A.MOD_ID, A.MOD_DT,
         dbo.sf_mat_nm(A.MAT_CD) AS MAT_NM, 
         ISNULL(B.STOCK_QTY, 0) AS STOCK_QTY,
         ISNULL(B.STOCK_AMT, 0) AS STOCK_AMT
    FROM TB_22020_H A,
         VI_29012_1 B
   WHERE A.MAT_CD = B.MAT_CD
     AND A.COLOR  = B.COLOR
     AND A.HOUSE  = B.HOUSE
     AND A.BRAND = :as_brand
     AND A.OUT_YMD = :as_yymmdd
     AND A.OUT_GUBN = '05'
     AND A.HOUSE    = :as_house
     AND A.CUST_CD  = :as_cust_cd
     AND A.MAT_GUBN = :as_mat_gubn
" update="TB_22020_H" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_yymmdd", string),("as_mat_gubn", string),("as_house", string),("as_cust_cd", string))  sort="mat_cd A color A spec A " )
