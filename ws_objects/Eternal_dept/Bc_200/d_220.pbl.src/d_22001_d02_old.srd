 column=(type=char(1) updatewhereclause=yes name=sil_chk dbname="SIL_CHK" values="	Y/	N" )
 column=(type=char(1) updatewhereclause=yes name=fin_yn dbname="FIN_YN" values="	Y/	N" )
 retrieve="  SELECT B.IN_YMD, 
         MIN(CASE ISNULL(B.SIL_QTY, 0) WHEN 0 THEN 'N' ELSE 'Y' END) AS SIL_CHK,
         ISNULL(MIN(A.FIN_YN), 'N') AS FIN_YN
    FROM TB_21011_D A,
         TB_22010_H B
   WHERE A.MAT_CD = B.MAT_CD
     AND A.COLOR  = B.COLOR
     AND A.MAT_CD  = :as_mmat_cd
     AND B.BRAND   = :as_brand
     AND B.IN_GUBN = '01'
     AND B.HOUSE   = :as_house
     AND B.IO_GUBN = '+'
   GROUP BY B.IN_YMD
   ORDER BY B.IN_YMD
" arguments=(("as_brand", string),("as_mmat_cd", string),("as_house", string))  sort="in_ymd A " )
