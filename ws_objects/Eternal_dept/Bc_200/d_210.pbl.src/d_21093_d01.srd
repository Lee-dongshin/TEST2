 retrieve="  SELECT TB_12029_D.STYLE,   
	TB_12029_D.CHNO,  
	TB_12029_D.COLOR,   
	TB_12029_D.MAT_GUBN1,   
	TB_12029_D.MAT_NM1,   
	TB_12029_D.MAT_RATE1,   
	TB_12029_D.MAT_GUBN2,   
	TB_12029_D.MAT_NM2,   
	TB_12029_D.MAT_RATE2,   
	TB_12029_D.MAT_GUBN3,   
	TB_12029_D.MAT_NM3,   
	TB_12029_D.MAT_RATE3,   
	TB_12029_D.MAT_GUBN4,   
	TB_12029_D.MAT_NM4,   
	TB_12029_D.MAT_RATE4,   
	TB_12029_D.MAT_GUBN5,   
	TB_12029_D.MAT_NM5,   
	TB_12029_D.MAT_RATE5,   
	TB_12029_D.MAT_GUBN6,   
	TB_12029_D.MAT_NM6,   
	TB_12029_D.MAT_RATE6, 
	TB_12029_D.MAT_GUBN7,   
	TB_12029_D.MAT_NM7,   
	TB_12029_D.MAT_RATE7,   
	TB_12029_D.MAT_GUBN8,   
	TB_12029_D.MAT_NM8,   
	TB_12029_D.MAT_RATE8,     
	TB_12029_D.REMARK1,   
	TB_12029_D.REMARK2,   
	TB_12029_D.REMARK3,   
	TB_12029_D.REMARK4,   
	TB_12029_D.REMARK5,
	TB_12029_D.MAKER_FG,   
	TB_12029_D.COUNTRY_CD,   
	TB_12029_D.WASHING1,   
	TB_12029_D.WASHING2,   
	TB_12029_D.WASHING3,   
	TB_12029_D.WASHING4,   
	TB_12029_D.CONFIRM_MARK,
	TB_12029_D.BOX_FG,
	TB_12029_D.REG_ID,   
	TB_12029_D.REG_DT,   
	TB_12029_D.MOD_ID,   
	TB_12029_D.MOD_DT, 
	isnull(label_agree,'N') 	  as label_agree,
	'      '    as  CARE_CODE,
	TB_12029_D.WASHING1 + '.bmp' as pic1,  
	TB_12029_D.WASHING2 + '.bmp' as pic2,  
	TB_12029_D.WASHING3 + '.bmp' as pic3,  
	TB_12029_D.WASHING4 + '.bmp' as pic4,
	'                         '  as washing_gubn,
	conform_yn,
	label_size,
	label_color,
	label_outyn,
	(select max(isnull(dbo.sf_inter_nm('216',n.washing_type),'') + ' ' + isnull(bigo,'')) 
		from tb_12025_d m(nolock), tb_21015_d n(nolock) 
		where m.mat_cd    = n.mat_cd
		and   m.mat_color = n.color
		and   m.style = TB_12029_D.style
		and   m.chno  = TB_12029_D.chno
		and   m.color = TB_12029_D.color
		and   m.mat_fg   = 'A' 
		and   m.mat_gubn = '1')	as whshing_info,
	dbo.SF_mat_country(style,'%')		as fabric_by,
	TB_12029_D.WASHING5,   
	TB_12029_D.WASHING6,
	TB_12029_D.WASHING5 + '.bmp' as pic5,  
	TB_12029_D.WASHING6 + '.bmp' as pic6,
	sect_nm  
    FROM TB_12029_D (nolock)
   WHERE TB_12029_D.STYLE = :AS_STYLE
	  AND TB_12029_D.CHNO  = :AS_CHNO
	  AND TB_12029_D.COLOR = :AS_COLOR 
" update="TB_12029_D" updatewhere=0 updatekeyinplace=yes arguments=(("AS_STYLE", string),("AS_CHNO", string),("AS_COLOR", string)) )
