 retrieve="  SELECT a.style,   
         a.test_date,
         a.test_detail,  
         a.mod_id,
			(select person_nm from tb_93010_m where person_id = a.mod_id) as mod_nm,   
         a.mod_dt,
			convert(varchar,a.mod_dt,111) as mod_dt1,
			a.temp_count
    FROM dbo.test_0226_temp a left outer join tb_93010_m b on a.reg_id = b.person_id
	WHERE a.style like :is_style
" arguments=(("is_style", string))  sort="style A temp_count A " )
