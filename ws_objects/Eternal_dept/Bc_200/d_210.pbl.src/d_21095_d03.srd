 retrieve="  SELECT d.style,   
         d.chno, 
  	      d.style + '-' + d.chno as style_no,
			d.tag_price 
    FROM vi_12020_1  d  with (nolock)
  where  d.brand = :as_brand
   and   d.year  = :as_year
	and   d.season = :as_season
   and   d.sojae like :as_sojae + '%'
	and   d.item like :as_item + '%'
   and   d.style like :as_style + '%'
	and   d.country_cd like :as_country_cd;
  
