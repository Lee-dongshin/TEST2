table(column=(type=char(8) updatewhereclause=yes name=yymmdd dbname="yymmdd" )
 column=(type=char(4) updatewhereclause=yes name=out_no dbname="out_no" )
 column=(type=char(36) updatewhereclause=yes name=shop_cd dbname="shop_cd" )
 column=(type=char(21) updatewhereclause=yes name=shop_type dbname="shop_type" )
 column=(type=decimal(0) updatewhereclause=yes name=out_qty dbname="out_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=rtrn_qty dbname="rtrn_qty" )
 retrieve="
select a.yymmdd, a.out_no,
       a.shop_cd + dbo.sf_shop_nm(a.shop_cd, 'a') as shop_cd, 
       a.shop_type + dbo.sf_inter_nm('009',a.shop_type) as shop_type, 
       sum(a.out_qty) as out_qty, sum(a.rtrn_qty) as rtrn_qty
from (	select yymmdd, out_no, shop_cd, shop_type, qty as out_qty, 0 as rtrn_qty
			from dbo.tb_42020_h
			where shop_cd 	= :as_shop_cd
     			and   style 	= :as_style
			and   color 	like :as_color
			and   size 		= :as_size
			and   brand 	= :as_brand
			and   chno  like :as_chno
			union all
			select yymmdd, out_no,shop_cd, shop_type, 0 as out_qty, qty as rtrn_qty
			from dbo.tb_42021_h
			where shop_cd 	= :as_shop_cd
      			and   style 	= :as_style
			and   color 	like :as_color
			and   size 		= :as_size
			and   brand 	= :as_brand
			and   chno  like :as_chno) a
group by a.yymmdd,a.out_no, a.shop_cd, a.shop_type


" arguments=(("as_shop_cd", string),("as_style", string),("as_color", string),("as_size", string),("as_brand", string),("as_chno", string)) )
