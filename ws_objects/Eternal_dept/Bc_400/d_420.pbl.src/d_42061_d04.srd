 retrieve="

select 	fr_shoP_cd, dbo.sf_shop_nm(fr_shop_cd,'s') as fr_shop_nm,
	to_shop_cd, dbo.sf_shop_nm(to_shop_cd,'s') as to_shop_nm,
	style + chno + color + size	as style_no, 
	sum(move_qty)	as move_qty , 
	dbo.sf_inter_nm('406',tran_type) 	as tran_type, 
	box_no
from tb_53020_h
where fr_ymd = :as_yymmdd
and  isnull(tran_cust,'XX') = :as_tran_cust
and  brand    = :as_brand
group by fr_shoP_cd, to_shop_cd, style + chno + color + size,  tran_type, box_no" arguments=(("as_yymmdd", string),("as_tran_cust", string),("as_brand", string)) )
