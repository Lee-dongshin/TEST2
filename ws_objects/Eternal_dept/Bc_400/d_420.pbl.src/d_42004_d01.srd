 column=(type=char(2) updatewhereclause=yes name=size dbname="tb_52031_h.size" )
 column=(type=decimal(0) updatewhereclause=yes name=deal_qty dbname="deal_qty" )
 column=(type=char(1) updatewhereclause=yes name=deal_fg dbname="tb_52031_h.deal_fg" )
 retrieve="select out_ymd, deal_seq, 
       style, chno,
       color + ' ' + dbo.sf_color_nm(color, 'k') as color, size,
       sum(deal_qty) as deal_qty,
		 deal_fg
from tb_52031_h with (nolock)
where style like :as_brand + '%'
and   out_ymd = :as_out_ymd
and   deal_seq = :ai_deal_seq
and   deal_fg = :as_deal_fg
and   proc_yn = :as_proc_yn
group by out_ymd, deal_seq, style, chno, color, size, deal_fg
having sum(deal_qty) > 0
order by 6,3" arguments=(("as_brand", string),("as_out_ymd", string),("ai_deal_seq", number),("as_deal_fg", string),("as_proc_yn", string)) )
