 retrieve="select YYMMDD, WORK_NO, 
       sum(OUT_qty) as OUT_QTY,
		 deal_fg
from beaucre.dbo.tb_52031_WORK with (nolock)
where YYMMDD = :AS_YYMMDD
AND 	BRAND  = :as_brand 
and   deal_fg  like :as_deal_fg + '%'
AND   TRX_CHK = '2'
and   ISNULL(OUT_TERM,'N') = :as_OUT_TERM
group by YYMMDD, WORK_NO, deal_fg
having sum(OUT_qty) > 0
order by 1,2" arguments=(("as_brand", string),("as_YYMMDD", string),("as_deal_fg", string),("as_OUT_TERM", string)) )
