 retrieve="select a.out_ymd, a.deal_seq, 
			B.YEAR, 
			B.SEASON, 
			DBO.SF_INTER_NM('003', B.SEASON) AS SEASON_NM,
			b.item, DBO.SF_ITEM_NM3(B.ITEM, :AS_BRAND, LEFT(:aS_OUT_YMD,4) ) AS ITEM_NM, 
			sum(deal_qty) AS DEAL_QTY,
	    a.out_ymd AS TO_OUT_YMD,	a.deal_seq as to_deal_seq  , 'N' AS PROC_YN
from tb_52031_h a (NOLOCK), tb_12020_m b (NOLOCK)
where a.out_ymd = :AS_OUT_YMD
and a.shop_cd like :AS_BRAND + '%'
and a.deal_seq = :AL_DEAL_SEQ
and a.style = b.style
group by a.out_ymd, a.deal_seq, b.item, B.YEAR, B.SEASON
" arguments=(("as_brand", string),("as_out_ymd", string),("al_deal_seq", number)) )
