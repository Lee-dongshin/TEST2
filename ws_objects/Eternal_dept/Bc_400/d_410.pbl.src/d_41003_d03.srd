 column=(type=decimal(0) updatewhereclause=yes name=a_in_qty dbname="a_in_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=b_in_qty dbname="b_in_qty" )
 retrieve="select a.size,
	       isnull(sum(a.qty), 0) day_in_qty,  
	       case when a.class = 'a' then isnull(sum(a.qty),0) 
                    else  0    end as a_in_qty,
	       case when a.class <> 'a' then isnull(sum(a.qty),0) 
                    else  0    end as b_in_qty	    
	from tb_41010_h a
	where a.brand = :as_brand
	and   a.yymmdd = :as_yymmdd
        and   a.in_no   = :as_in_no
        and   a.cust_cd  = :as_cust_cd
        and   a.style  = :as_style
        and   a.chno   = :as_chno
        and   a.color  = :as_color
	group by a.size, a.class" arguments=(("as_brand", string),("as_yymmdd", string),("as_in_no", string),("as_cust_cd", string),("as_style", string),("as_chno", string),("as_color", string)) )
