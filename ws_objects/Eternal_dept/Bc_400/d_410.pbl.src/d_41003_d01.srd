 column=(type=decimal(0) updatewhereclause=yes name=day_in_qty dbname="day_in_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=a_in_qty dbname="a_in_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=b_in_qty dbname="b_in_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=ord_qty dbname="ord_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=tot_ain_qty dbname="tot_ain_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=tot_bin_qty dbname="tot_bin_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=add_qty dbname="add_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=make_amt dbname="make_amt" )
 retrieve="select a.cust_cd , dbo.sf_cust_nm(a.cust_cd, 'a') as cust_nm,
       isnull(sum(a.day_in_qty), 0)  as day_in_qty,
       isnull(sum(a.a_in_qty), 0)    as a_in_qty,
       isnull(sum(a.b_in_qty), 0)    as b_in_qty,
       isnull(sum(a.ord_qty), 0)     as ord_qty,
       isnull(sum(a.tot_ain_qty), 0) as tot_ain_qty,
       isnull(sum(a.tot_bin_qty), 0) as tot_bin_qty,
       isnull(sum(a.add_qty), 0)     as add_qty,                     
       isnull(sum(a.make_amt), 0)    as make_amt
from 	(select a.cust_cd, a.style, a.chno, a.color, a.size, 
	       sum(a.qty) day_in_qty,  
	       case when a.class = 'a' then sum(a.qty) end as a_in_qty,
	       case when a.class <> 'a' then sum(a.qty) end as b_in_qty,
	       b.ord_qty, 
	       b.in_qty  as tot_ain_qty,
	       b.bad_qty as tot_bin_qty,
	       b.add_qty,
	       b.make_amt
	from tb_41010_h a, tb_12030_s b
	where a.brand = :as_brand 
	and   a.yymmdd = :as_yymmdd
   and   a.cust_cd like :as_cust_cd
	and   a.style  = b.style
	and   a.chno   = b.chno
	and   a.color  = b.color
	and   a.size   = b.size
	group by a.cust_cd, a.style, a.chno, a.color, a.size, a.class,
	       b.ord_qty, 
	       b.in_qty,
	       b.bad_qty,
	       b.add_qty,
	       b.make_amt)  a
group by a.cust_cd" arguments=(("as_brand", string),("as_yymmdd", string),("as_cust_cd", string)) )
