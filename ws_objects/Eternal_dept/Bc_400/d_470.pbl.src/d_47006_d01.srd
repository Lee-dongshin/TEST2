 retrieve="


select 	ord_ymd, shop_cd, dbo.sf_shop_nm(shop_cd, 's') as shop_nm,
	order_no, no, proc_stat ,rtrn_stat,
	order_name, order_mobile, order_tel,
	receiver_name, receiver_mobile, receiver_tel,
	style, chno, color, dbo.sf_color_nm(color ,'e')	AS COLOR_NM, size, qty,
	case when left(style,1) = 'E' then 0  else dbo.sf_house_real_stock(style, '%', color, size, '010000') end	as house_stock,
	dbo.sf_shop_stock2( left(style,1) + 'D1900', '%', style,'%',color,size) as eshop_stock,
	erp_sale_price,
	sale_ymd	, yymmdd, gubn,
   'N'	as sale_yn, rtrn_sale_ymd, rtrn_sale_price,rtrn_reason,
	tran_bill,rtrn_work_ymd,
rtrn_invoice_no

from tb_45030_h (nolock)
where ord_ymd between :as_fr_ymd and :as_to_ymd
and   rtrn_stat like :as_rtrn_stat + '%'
and   shoP_cd like :as_shop_cd + '%'
and   proc_stat = '02'	
and   gubn = 'B'
and   rtrn_stat in ('00','01','02') 
and   tran_bill > 0
and   isnull(rtrn_work_ymd,'00000000') between :as_fr_rtrn_ymd and :As_to_rtrn_ymd
and	mall_gubn like :as_mall_gubn" update="tb_45030_h" updatewhere=0 updatekeyinplace=no arguments=(("as_fr_ymd", string),("as_to_ymd", string),("as_shop_cd", string),("as_rtrn_stat", string),("as_fr_rtrn_ymd", string),("as_to_rtrn_ymd", string),("as_mall_gubn", string))  sort="rtrn_work_ymd A ord_ymd A shop_cd A order_no A " )
