 retrieve="select  a.shop_seq, a.shop_snm, a.shop_seq + a.shop_snm as shop_gubn
from 
(	select  distinct shop_seq, 
		replace(replace(replace (replace(replace(replace(shop_snm, '(Netc)',''),'(e-온앤온)',''),'(온앤온)',''),'(올리브)',''),'(lapalette)',''),'(stage89)','') as shop_snm
		from tb_91100_m
		where shop_stat = '00'
		and   ( shop_div in ('D','E') 
			or ( brand= 'E' and shop_div = 'G')
			or ( brand= 'B' and (shop_div = 'G' or shop_div = 'M')))

		union all
	select  distinct shop_seq, 
		replace(replace(replace (replace(replace(replace(shop_snm, '(Netc)',''),'(e-온앤온)',''),'(온앤온)',''),'(올리브)',''),'(lapalette)',''),'(stage89)','') as shop_snm
		from tb_91100_m
		where shop_stat = '00'
		and   brand in ('E','S','B','P') and shop_div in ('K','B')
) a" )
