 retrieve="  SELECT yymmdd,   
         shop_cd,
			dbo.sf_shop_nm(shop_cd, 'S') AS SHOP_NM, 
         shop_type,   
         sale_no,   
         no,   
         style + chno as style_no, 
         style,   
         chno,   
         color,   
         size,   
         sale_type,   
         sale_qty,   
         tag_price,   
         curr_price,   
         dc_rate,   
         sale_price,   
         tag_amt,   
         curr_amt,   
         sale_amt,   
         out_rate,   
         out_amt,   
         sale_rate,   
         sale_collect,   
         io_amt,   
         io_vat,   
         age_grp,   
         sale_fg,   
         jumin,   
         card_no, 
         right(card_no, 9) as card_no2,   
         goods_amt,   
         sale_cust,   
         brand,   
         year,   
         season,   
         sojae,   
         item,   
         shop_div,   
         reg_id,   
         reg_dt,   
         mod_id,   
         mod_dt, 
         out_amt      / sale_qty as out_price, 
         sale_collect / sale_qty as collect_price,
			coupon_no   ,
			case when isnull(card_no,'') <> '' and left(sale_type,1) = '1'  then 'Y' else 'N' end as mem_dc_yn,
			empty_1,
			empty_5,
			empty_6,
			phone_no,
			case when empty_1 = 'N' then '1' else '0' end as flag
    FROM tb_53017_h   
   WHERE yymmdd = :as_yymmdd 
     AND shop_cd   = :as_shop_cd 
     AND shop_type = :as_shop_type
	  AND sale_no = :as_sale_no
	  AND no like :as_no" update="tb_53017_h" updatewhere=0 updatekeyinplace=no arguments=(("as_yymmdd", string),("as_shop_cd", string),("as_shop_type", string),("as_sale_no", string),("as_no", string)) )
