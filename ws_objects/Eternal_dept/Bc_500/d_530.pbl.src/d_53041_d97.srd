table(column=(type=char(4) updatewhereclause=yes name=shop_seq dbname="shop_seq" )
 column=(type=char(255) updatewhereclause=yes name=shop_snm dbname="shop_snm" )
 column=(type=char(255) updatewhereclause=yes name=shop_gubn dbname="shop_gubn" )
 retrieve="select  a.shop_seq, a.shop_snm, a.shop_seq + a.shop_snm as shop_gubn
from 
(	select  distinct shop_seq, 
		replace(replace(replace (replace(replace(replace(shop_snm, '(L-B라빠)',''),'(JOY RTW)',''),'(JOY)',''),'(raive)',''),'(lapalette)',''),'(RTW)','') as shop_snm
		from tb_91100_m
		where shop_stat = '00'
		and   ( shop_div in ('D','E') 
			or ( brand= 'E' and shop_div = 'G')
			or ( brand= 'B' and (shop_div = 'G' or shop_div = 'M'))
			or ( brand= 'L' and (shop_div = 'G' or shop_div = 'M' or shop_div = 'H'))
			or ( brand= 'K' and (shop_div = 'G' or shop_div = 'M' or shop_div = 'H'))
			or ( brand= 'V' and (shop_div = 'G' or shop_div = 'M' or shop_div = 'H'))			
			)

		union all
	select  distinct shop_seq, 
		replace(replace(replace (replace(replace(replace(shop_snm, '(L-B라빠)',''),'(JOY RTW)',''),'(JOY)',''),'(raive)',''),'(lapalette)',''),'(RTW)','') as shop_snm
		from tb_91100_m
		where shop_stat = '00'
		and   brand in ('E','S','B','P','V','K') and shop_div in ('K','B')
) a
order by a.shop_seq" )
