 column=(type=decimal(0) updatewhereclause=yes name=real_sale_amt dbname="real_sale_amt" )
 column=(type=decimal(0) updatewhereclause=yes name=tag_amt dbname="tag_amt" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_collect dbname="sale_collect" )
 column=(type=decimal(0) updatewhereclause=yes name=wonga dbname="wonga" )
 retrieve="select a.style ,a.chno, 
		a.color, dbo.sf_color_nm(a.color,'e') as color_nm,
		a.size,
		sum(a.sale_qty)		as sale_qty, 
		sum(a.real_sale_amt)	as real_sale_amt, 
		sum(a.tag_amt)			as tag_amt, 
		sum(a.sale_collect) 	as sale_collect, 
      isnull(b.mmat_price,0) +isnull(b.smat_price,0) +isnull(b.make_price,0) +isnull(b.etc_price,0)  as wonga
from tb_53010_h a (nolock), tb_12021_d b (nolock)
where a.yymmdd between :as_frm_ymd and :As_to_ymd
and b.make_type = '50'
and b.brand = :as_brand
and b.style like :As_style + '%'
and b.chno  like :as_chno + '%'
and b.year  like :as_year + '%'
and b.season  like :as_season + '%'
and a.style = b.style
and a.chno  = b.chno
group by a.style ,a.chno, a.color, a.size, b.mmat_price, b.smat_price, b.make_price, b.etc_price
" arguments=(("as_frm_ymd", string),("as_to_ymd", string),("as_brand", string),("as_style", string),("as_chno", string),("as_year", string),("as_season", string)) )
