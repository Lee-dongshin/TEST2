 retrieve="  SELECT yymmdd,SHOP_CD, dbo.sf_shop_nm(SHOP_CD, 'S') AS SHOP_NM,
         CASE SHOP_TYPE 
          WHEN '1' THEN 'A' 
          WHEN '3' THEN 'A1' END AS OTHR_BRAND,
         CASE SHOP_TYPE 
          WHEN '1' THEN '정상매출' 
          WHEN '3' THEN '기획매출' END AS OTHR_BRAND_NM,
         ISNULL(SUM(SALE_AMT), 0) AS SALE_AMT
    FROM TB_53010_H  WITH (NOLOCK)
   WHERE YYMMDD between :as_sale_dt and :as_sale_dt1
     AND SHOP_CD LIKE :as_brand + :as_shop_div + :as_shop_cd
     AND SHOP_TYPE IN ('1', '3')
   GROUP BY SHOP_CD, SHOP_TYPE, yymmdd

  UNION ALL 

  SELECT SALE_YMD,SHOP_CD, dbo.sf_shop_nm(SHOP_CD, 'S') AS SHOP_NM,
			OTHR_BRAND, dbo.sf_inter_nm('012', OTHR_BRAND) AS OTHR_BRAND_NM,
			ISNULL(SALE_AMT, 0) AS SALE_AMT
  FROM TB_53030_H WITH (NOLOCK)
   WHERE SALE_YMD between :as_sale_dt and :as_sale_dt1
     AND SHOP_CD LIKE :as_brand + :as_shop_div + :as_shop_cd
  and   OTHR_BRAND IN (select  INTER_CD from tb_91011_c
								WHERE INTER_GRP = '012'
								AND   INTER_CD2 = 'Y'
								AND   INTER_CD  LIKE :AS_BRAND + '__')  
  
   UNION ALL

  SELECT distinct a.SALE_YMD, a.SHOP_CD, dbo.sf_shop_nm(a.SHOP_CD, 'S') 						AS SHOP_NM,
  	                upper(:as_brand) + '51' 				  									as OTHR_BRAND, 
                   dbo.sf_inter_nm('012', :as_brand + '51')    						AS OTHR_BRAND_NM,
                	 (select sum(isnull(c.real_sale_amt,0)) from tb_44020_s c WITH (NOLOCK)
               	  where	c.yymmdd = a.sale_ymd
                    and   c.shop_cd = b.in_shop_cd)	  								AS SALE_AMT
   FROM TB_53030_H a WITH (NOLOCK) , tb_53031_M b
   whERE a.SALE_YMD between :as_sale_dt and :as_sale_dt1
    AND a.SHOP_CD LIKE :as_brand + :as_shop_div + :as_shop_cd
   and   a.shop_cd = b.shop_cd
   	 " arguments=(("as_brand", string),("as_sale_dt", string),("as_sale_dt1", string),("as_shop_cd", string),("as_shop_div", string))  sort="yymmdd A shop_cd A shop_nm A " )
