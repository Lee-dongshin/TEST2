 retrieve="  SELECT a.style,   
         a.chno,   
         case when convert(char(08), a.reg_dt,112) < '20031104' and  a.shop_cd = 'NZ1999' then 'NT3516'
              else a.shop_cd end as shop_CD  ,   
         a.color,   
         a.size,   
         a.deal_qty,   
         a.out_ymd,   
         a.reg_id,   
         a.mod_id,   
         a.mod_dt, 
         b.shop_snm  as shop_nm, 
         b.shop_stat,
		   b.deal_yn   as deal_yn,
			b.shop_lv	as shop_lv,
			b.area_cd	as area_cd,
			b.shop_lv	as shop_lv1
    FROM tb_52030_m a, 
         tb_91100_m b 
   WHERE b.shop_cd = a.shop_cd
     AND a.style = :as_style 
     AND a.chno  = :as_chno 
     and a.color = :as_color
	  and b.deal_yn   = 'Y'
     AND B.PLAN_DEAL_YN LIKE CASE WHEN SUBSTRING(:as_style, 5,1) = 'Z' THEN 'Y%'
	 			            ELSE '%' END
   ORDER BY b.shop_div, b.area_cd, a.shop_cd, a.color, a.size " update="tb_52030_m" updatewhere=0 updatekeyinplace=no arguments=(("as_style", string),("as_chno", string),("as_color", string)) )
