table(column=(type=char(2) updatewhereclause=yes name=color dbname="color" )
 column=(type=char(2) updatewhereclause=yes name=size dbname="size" )
 column=(type=decimal(0) updatewhereclause=yes name=ord_qty dbname="ord_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=ord_deal dbname="ord_deal" )
 retrieve=" select b.color    as color, 
         b.size     as size, 
         a.ord_qty - a.ord_qty_chn  as ord_qty, 
         convert(decimal(8), (a.ord_qty - a.ord_qty_chn) * c.resv_rate / 100 ) as ord_deal 
    from tb_12030_s a with (nolock), 
         tb_12024_d b with (nolock), 
         (select 100 - isnull(dd.resv_rate, ee.resv_rate) as resv_rate
            from tb_12021_d dd with (nolock), 
                 (select isnull(convert(decimal(3), inter_data1), 0) as resv_rate
                    from tb_91011_c 
                    where inter_grp = '001' 
                     and inter_cd  = left(:as_style, 1)) ee
           where dd.style = :as_style 
             and dd.chno  = :as_chno) c  
   where a.style = b.style 
     and a.chno  = b.chno 
     and a.color = b.color 
     and a.size  = b.size 
     and b.style = :as_style 
     and b.chno  = :as_chno 
     and a.color in (:as_color1, :as_color2, :as_color3, :as_color4)
   order by 1, 2" arguments=(("as_style", string),("as_chno", string),("as_color1", string),("as_color2", string),("as_color3", string),("as_color4", string)) )
