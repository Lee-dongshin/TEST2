 retrieve="  select shop_cd, 
         shop_snm as shop_nm, 
         empno, 
         dbo.sf_emp_nm(empno)  as emp_nm, 
         deal_yn, 
         refill_yn,
			plan_deal_yn,
			area_cd, shop_div,
			refill_benifit
    from tb_91100_m 
   where brand     =    :as_brand 
     and shop_div  LIKE :as_shop_div 
     and shop_stat = '00' 
     and shop_div  in  ('G', 'K', 'D','Z','E','B')
 union all
  select shop_cd, 
         shop_snm as shop_nm, 
         empno, 
         dbo.sf_emp_nm(empno)  as emp_nm, 
         deal_yn, 
         refill_yn,
			plan_deal_yn,
			area_cd, shop_div,
			refill_benifit
    from tb_91100_m 
   where brand     =    :as_brand 
     and shop_div  LIKE :as_shop_div 
     and shop_stat = '00' 
     and  SHOP_CD IN ('NX3300','oX3300','WX3300','NX4997','OX4997','WX4997','OX4994','WX4994','NX4994','NX4996','OX4996','WX4996','NX4988','OX4988','WX4988','OX4993','NX4989', 'NX4999','OX4999','NX4983','OX4983', 'NX4986','OX4986','NX1000','OX1000','IX4997','OX4987','IX4986')
   order by shop_div, area_cd, shop_cd 
" update="tb_91100_m" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_shop_div", string)) )
