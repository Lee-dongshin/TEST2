 retrieve="  SELECT TB_51063_d.yymmdd,   
         TB_51063_d.no,   
         TB_51063_d.inter_cd,  
			dbo.sf_inter_nm('094', TB_51063_d.inter_cd) inter_nm,
         TB_51063_d.shop_cd,   
			dbo.sf_shop_nm(TB_51063_d.shop_cd, 'S') shop_nm,
         TB_51063_d.amt,
         TB_51063_d.note,   
         TB_51063_d.reg_id,   
         TB_51063_d.reg_dt,   
         TB_51063_d.mod_id,   
         TB_51063_d.mod_dt  
    FROM TB_51063_d 	left outer join tb_91100_m b with (nolock) on TB_51063_d.shop_cd = b.shop_cd
	where left(TB_51063_d.shop_cd,1) like '%' + :as_brand
			and TB_51063_d.yymmdd like :as_yymmdd + '%'
			and b.empno like '%' + :as_empno


