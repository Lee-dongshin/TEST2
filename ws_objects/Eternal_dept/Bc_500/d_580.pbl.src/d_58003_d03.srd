 retrieve="	SELECT Brand,   
		Invoice_Date,   
		Invoice_No, 
		seq_no, 
		item_nm,  
		Style,   
		Composition,   
		sale_qty,   
		Exchange_price,   
		exchange_amt,   
		sale_amt,   
		country_cd,
		reg_id,   
		reg_dt,   
		mod_id,   
		mod_dt,
		chno,
		(select top 1 1 from tb_12029_e (nolock)		
	     where style = a.style) as inner_yn,
		color,
		dbo.sf_color_nm(color,'e')	as color_nm	 ,
	   isnull(size,'') as size,
      (select smat_info from tb_12020_m (nolock)		
	     where style = a.style) as smat_info 
	FROM tb_56201_h  a(nolock)
	WHERE a.Brand = :AS_BRAND
	AND  a.Invoice_Date = :AS_INVOICE_DATE
	AND  a.Invoice_No   = :AS_INVOICE_NO;
" update="tb_56201_h" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_invoice_date", string),("as_invoice_no", string)) )
