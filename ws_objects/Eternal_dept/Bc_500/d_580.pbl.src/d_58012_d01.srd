 column=(type=decimal(0) updatewhereclause=yes name=l1 dbname="L1" )
 retrieve="select	aa.factory_cd,
			sum(aa.A) as A1,
			sum(aa.B) as B1,
			sum(aa.C) as C1,
			sum(aa.D) as D1,
			sum(aa.E) as E1,
			sum(aa.F) as F1,
			sum(aa.G) as G1,
			sum(aa.H) as H1,
			sum(aa.I) as I1,
			sum(aa.J) as J1,
			sum(aa.K) as K1,
			sum(aa.L) as L1
from (
		select	max(b.factory_cd) as factory_cd, 
				case when substring(yymmdd,5,2) = '01' then sum(isnull(real_make_amt, 0)) else 0 end as A,
				case when substring(yymmdd,5,2) = '02' then sum(isnull(real_make_amt, 0)) else 0 end as B,
				case when substring(yymmdd,5,2) = '03' then sum(isnull(real_make_amt, 0)) else 0 end as C,
				case when substring(yymmdd,5,2) = '04' then sum(isnull(real_make_amt, 0)) else 0 end as D,
				case when substring(yymmdd,5,2) = '05' then sum(isnull(real_make_amt, 0)) else 0 end as E,
				case when substring(yymmdd,5,2) = '06' then sum(isnull(real_make_amt, 0)) else 0 end as F,
				case when substring(yymmdd,5,2) = '07' then sum(isnull(real_make_amt, 0)) else 0 end as G,
				case when substring(yymmdd,5,2) = '08' then sum(isnull(real_make_amt, 0)) else 0 end as H,
				case when substring(yymmdd,5,2) = '09' then sum(isnull(real_make_amt, 0)) else 0 end as I,
				case when substring(yymmdd,5,2) = '10' then sum(isnull(real_make_amt, 0)) else 0 end as J,
				case when substring(yymmdd,5,2) = '11' then sum(isnull(real_make_amt, 0)) else 0 end as K,
				case when substring(yymmdd,5,2) = '12' then sum(isnull(real_make_amt, 0)) else 0 end as L
		from tb_41010_h a with (nolock) left outer join tb_12021_d b with (nolock) on a.style+a.chno = b.style+b.chno
		where	a.brand like :as_brand + '%'
				and left(a.yymmdd,4) = :as_yyyy
				and a.year like :as_year + '%'
				and a.season like :as_season  +'%'
				and a.cust_cd = 'N08906'
				and b.country_cd <> '00' 
		group by b.factory_cd, substring(yymmdd,5,2)
		) aa
group by aa.factory_cd
order by aa.factory_cd" arguments=(("as_brand", string),("as_yyyy", string),("as_year", string),("as_season", string)) )
