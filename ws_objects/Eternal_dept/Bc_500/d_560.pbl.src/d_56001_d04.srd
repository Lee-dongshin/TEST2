 retrieve="  SELECT tb_53010_sale_shop.Shop_Cd,   
         tb_53010_sale_shop.Sale_Type,   
         tb_53010_sale_shop.End_Ymd,   
         tb_53010_sale_shop.Start_Ymd, 
         tb_53010_sale_shop.Reg_id,  
         tb_53010_sale_shop.Reg_dt,   
         tb_53010_sale_shop.Mod_id,   
         tb_53010_sale_shop.Mod_Dt  
    FROM tb_53010_sale_shop with (nolock)  
   WHERE tb_53010_sale_shop.SHOP_CD = :as_shop_cd 
     AND left(tb_53010_sale_shop.shop_cd,1)   = :as_brand
     AND tb_53010_sale_shop.start_Ymd <= :as_yymmdd
     AND tb_53010_sale_shop.end_Ymd >= :as_yymmdd 
" update="tb_53010_sale_shop" updatewhere=0 updatekeyinplace=yes arguments=(("as_shop_cd", string),("as_brand", string),("as_yymmdd", string)) )
