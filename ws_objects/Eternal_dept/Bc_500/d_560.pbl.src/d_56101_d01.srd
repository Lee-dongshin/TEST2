 retrieve="SELECT A.YYMMDD,
		 A.SHOP_CD,
		 A.DEPOSIT_NO,
		 A.NO,
		 A.DEPOSIT_FG,
		 A.ACC_CD,
		 B.SNAME,
		 A.DEPOSIT_AMT,
		 A.DRAFT_NO,
		 A.ISSUE_YMD,
		 A.ISSUE_NM, 
		 A.FULL_YMD,
		 A.BANK_CD,
		 A.REMARK,
		 A.SLIP_BONJI,
		 A.SLIP_DATE,
		 A.SLIP_NO,
		 A.BRAND,
		 A.SHOP_DIV,
		 A.REG_ID,
		 A.REG_DT,
		 A.MOD_ID,
		 A.MOD_DT
  FROM TB_56040_H    A with (nolock), 
       MIS.DBO.TAB01 B with (nolock)
 WHERE A.BRAND    = :as_brand
   AND A.YYMMDD   = :as_yymmdd
   AND A.DEPOSIT_NO LIKE :as_deposit_no + '%'
   AND A.SHOP_CD    LIKE :as_shop_cd  + '%'
   AND A.ACC_CD  *= B.ACC_CODE
ORDER BY A.NO  " update="TB_56040_H" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_yymmdd", string),("as_deposit_no", string),("as_shop_cd", string)) )
