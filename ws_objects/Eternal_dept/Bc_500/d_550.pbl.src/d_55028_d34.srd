table(column=(type=char(255) updatewhereclause=yes name=shop_nm dbname="shop_nm" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_qty dbname="sale_qty" )
 retrieve="select top 8 replace(replace(dbo.sf_shop_nm(shop_cd, 's'),'(올리브)',''),'(W.)','') as shop_nm, 
 case when :As_sale_gubn = '0' then sum(isnull(sale_qty,0)) - sum(isnull(sale_qty9,0))
		when :As_sale_gubn = '1' then sum(isnull(sale_qty1,0)) 
		else sum(isnull(sale_qty,0)) end as sale_qty
from tb_44012_s
where style    = :As_style
and   chno  like :as_chno + '%'
and   shop_div in ('G','K','D')
group by shop_cd
having  case when :As_sale_gubn = '0' then sum(isnull(sale_qty,0)) - sum(isnull(sale_qty9,0))
		when :As_sale_gubn = '1' then sum(isnull(sale_qty1,0)) 
		else sum(isnull(sale_qty,0)) end  > 0
order by 2 desc" arguments=(("as_style", string),("as_chno", string),("as_sale_gubn", string)) )
