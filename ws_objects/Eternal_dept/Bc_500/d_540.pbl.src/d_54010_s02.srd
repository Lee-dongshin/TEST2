 retrieve="
select 
yymmdd,
Shop_CD,
dbo.sf_shop_nm(shop_cd,'s')	as rcv_shop_nm,
Rqst_Seq,
Rt_Shop,
case when substring(Rt_Shop,2,6) = 'G0000' then
	 			'RT출고'
	  else	
		dbo.sf_shop_nm(rt_shop,'s')
end 	as out_shop_nm,
Rt_ymd,
Rqst_Qty,
Accept_Qty,
Accept_FG,
ACCEPT_YMD,
Shop_Type,
Rt_Shop_Type,
style,
chno,
color,
size,
Proc_yn,
Reg_id,
Reg_Dt,
Mod_id,
Mod_Dt
from tb_54031_d
where rt_ymd = :as_yymmdd
  and substring(shop_cd, 1,1) = :as_brand
  and isnull(proc_yn,'%') like :as_proc_yn + '%'
  and rqst_qty <> 0	
  and substring(shop_cd, 2,5) <> 'D1900'" update="tb_54031_d" updatewhere=1 updatekeyinplace=no arguments=(("as_yymmdd", string),("as_brand", string),("as_proc_yn", string)) )
