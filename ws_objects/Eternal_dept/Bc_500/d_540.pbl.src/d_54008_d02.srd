 column=(type=datetime updatewhereclause=yes name=mod_dt dbname="tb_54021_h.mod_dt" )
 column=(type=decimal(0) updatewhereclause=yes name=tag_price dbname="TAG_PRICE" )
 column=(type=decimal(6) updatewhereclause=yes name=sale_price dbname="SALE_PRICE" )
 retrieve="   SELECT A.style,   
         A.brand,   
         A.year,   
         A.season,   
         A.sojae,   
         A.item,   
         A.out_seq,   
         A.dep_seq,   
         A.dep_ymd,   
         A.dep_fg,
         (case   A.dep_fg  when 'y'  then  '품목할인'
                        when 'o'  then  '시즌Out'
                        when 'n'  then  '미확정'
         end) as dep_name,
         A.sel_yn,   
         A.rtrn_yn,   
         A.reg_id,   
         A.reg_dt,   
         A.mod_id,   
         A.mod_dt, 
			DBO.SF_FIRST_PRICE(A.STYLE)		AS TAG_PRICE,
	       (SELECT case when max(isnull(dc_rate,0)) = 0 then min(b.sale_price)
 		   		  else dbo.sf_first_price(B.style) * (100 - max(dc_rate)) / 100 end
	        FROM tb_56012_d B (NOLOCK)
      	  WHERE B.STYLE = A.STYLE 
           and   convert(char(08), getdate(),112) between start_ymd and end_ymd
			  and b.shop_cd  like case when b.brand = 'M' then '_E%' else '_G%' end
			 group by b.style
	 )   AS SALE_PRICE
    FROM tb_54021_h A (NOLOCK)  
   WHERE A.brand    =  :as_brand
     AND A.year     =  :as_year
     AND A.SEASON   =  :as_season
	  AND A.ITEM     <> CASE WHEN :AS_PLAN_EXCEPT = 'Y' THEN 'Z' ELSE '0' END
     AND A.SOJAE LIKE  :as_sojae + '%'
     AND A.ITEM  LIKE  :as_item + '%'
     AND A.DEP_SEQ LIKE :as_dep_seq + '%'   
     and A.DEP_FG  LIKE :as_dep_fg + '%'  
" update="tb_54021_h" updatewhere=0 updatekeyinplace=no arguments=(("as_brand", string),("as_year", string),("as_season", string),("as_sojae", string),("as_item", string),("as_dep_seq", string),("as_dep_fg", string),("as_plan_except", string))  sort="dep_seq A out_seq A style A " )
