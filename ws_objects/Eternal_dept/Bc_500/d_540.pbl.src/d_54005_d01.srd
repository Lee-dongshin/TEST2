 retrieve="  SELECT  A.DEP_FG, D.OUT_SEQ, A.STYLE,
			 isnull(SUM(B.IN_QTY),0) + isnull(SUM(B.IWOL_QTY),0) AS IN_QTY, 
			 isnull(SUM(B.OUT_QTY),0) AS OUT_QTY, 
          isnull(SUM(B.ETC_QTY),0) AS ETC_QTY, 
			 isnull(SUM(B.SALE_QTY_1),0) AS SALE_QTY1, 
          isnull(SUM(B.SALE_QTY_2),0) AS SALE_QTY2, 
          isnull(SUM(B.SALE_QTY_3),0) AS SALE_QTY3, 
          isnull(SUM(B.SALE_QTY_4),0) AS SALE_QTY4, 
			 isnull(SUM(B.SALE_QTY),0) AS SALE_QTY, 
          MIN(B.OUT_YMD) AS OUT_YMD, 
			 MIN(B.SALE_YMD) AS SALE_YMD,
          MIN(A.DEP_YMD)  AS DEP_YMD,
          MIN(A.DEP_SEQ)  AS DEP_SEQ,
			 a.dotcom 			                                         
    FROM TB_54020_H A,   
         TB_12030_S B,   
         TB_12020_M D WITH (NOLOCK)  
   WHERE  A.BRAND   =    :as_brand 
     AND  A.YEAR    =    :as_year  
     AND  A.SEASON  =    :as_season 
     AND  A.SOJAE   LIKE :as_sojae + '%' 
     AND  A.ITEM    LIKE :as_item + '%' 
     AND  A.DEP_FG  NOT IN ('Y', 'O')
     AND  A.STYLE   =    D.STYLE  
     AND  D.OUT_SEQ LIKE :as_out_seq + '%' 
     AND  A.STYLE   =    B.STYLE   
    GROUP BY A.DEP_FG, D.OUT_SEQ, A.STYLE, a.dotcom
    ORDER BY A.DEP_FG, D.OUT_SEQ, A.STYLE
" arguments=(("as_brand", string),("as_year", string),("as_season", string),("as_sojae", string),("as_item", string),("as_out_seq", string)) )
