 retrieve="select 
       a.style, a.chno, a.color + ' ' + dbo.sf_color_nm(a.color, 'k') as color, 
       a.size,
       sum(isnull(a.move_qty,0))   move_qty,
       to_shop_cd + ' ' + dbo.sf_shop_nm( to_shop_cd, 'S')
	from tb_54013_h a
	where a.brand        = :as_brand
   and   a.fr_shop_cd      = :as_shop_cd
  	and   a.yymmdd       = :as_yymmdd
   and   a.proc_yn    = 'y'
	group by a.style, a.chno,  a.color, a.size, a.to_shop_cd
	
" arguments=(("as_brand", string),("as_yymmdd", string),("as_shop_cd", string)) )
