 retrieve="  SELECT A.style,   
         A.brand,   
         A.year,   
         A.season,   
         A.sojae,   
         A.item,   
         A.out_seq,   
         A.dep_seq,   
         A.dep_ymd,   
         A.dep_fg,
         (case   A.dep_fg  when 'y'  then  '부진'
                        when 'o'  then  '시즌Out'
                        when 'n'  then  '미확정'
         end) as dep_name,
         A.sel_yn,   
         A.rtrn_yn,   
         A.reg_id,   
         A.reg_dt,   
         A.mod_id,   
         A.mod_dt, 
			DBO.SF_FIRST_PRICE(A.STYLE)		AS TAG_PRICE,
	       (SELECT CASE WHEN  MIN(B.SALE_PRICE) <=0 THEN MAX(B.SALE_PRICE) 
                ELSE  MIN(B.SALE_PRICE) END
	        FROM tb_56012_d B (NOLOCK)
      	  WHERE B.STYLE = A.STYLE )  AS SALE_PRICE,
	a.dotcom,
			a.dotcom_cancel,
			a.cancel_ymd
    FROM tb_54020_h A (NOLOCK)  
   WHERE A.brand    =  :as_brand
     AND A.year     =  :as_year
     AND A.SEASON   =  :as_season
     AND A.SOJAE LIKE  :as_sojae + '%'
     AND A.ITEM  LIKE  :as_item + '%'
     AND A.DEP_SEQ LIKE :as_dep_seq + '%'   
     and A.DEP_FG  LIKE :as_dep_fg + '%'  
" update="tb_54020_h" updatewhere=1 updatekeyinplace=no arguments=(("as_brand", string),("as_year", string),("as_season", string),("as_sojae", string),("as_item", string),("as_dep_seq", string),("as_dep_fg", string))  sort="dep_seq A out_seq A style A " )
