 retrieve="  SELECT A.YYMMDD, 
         A.SHOP_CD, 
         dbo.sf_shop_nm(A.SHOP_CD, 'S') 				AS SHOP_NM,
         A.RQST_SEQ, 
         A.SHOP_TYPE, 
         dbo.sf_inter_nm('911', A.SHOP_TYPE) 		AS SHOP_TYPE_NM,
         A.STYLE + A.CHNO 									AS STYLE_NO,
         A.COLOR, dbo.sf_color_nm(A.COLOR, 'E') 	AS COLOR_NM,
         A.SIZE, dbo.sf_size_nm(A.SIZE) 				AS SIZE_NM,
         dbo.sf_first_price(A.STYLE) 					AS TAG_PRICE,
         ISNULL(A.RQST_QTY, 0) 							AS RQST_QTY,  
         dbo.sf_house_stock(a.style, '%', a.color, a.size, '010000') as stock_qty,
 	      ISNULL(A.ACCEPT_QTY, 0) 						AS ACCEPT_QTY, 
         ISNULL(B.RQST_QTY, 0) 							AS RQST_QTY1, 
 		   ISNULL(B.ACCEPT_QTY, 0) 						AS ACCEPT_QTY1,

         case when a.out_ymd is not null and a.accept_yn = 'Y' then
         a.out_ymd
         else  B.RT_YMD end				as rt_ymd, 

         A.ACCEPT_YN, A.TAEK_YN, A.SELF_YN, 

         case when a.out_ymd is not null and a.accept_yn = 'Y' then 
         '010000'
         else B.RT_SHOP   end				as rt_shop, 

         case when a.out_ymd is not null and a.accept_yn = 'Y' then 

         '물류'
         else  dbo.sf_shop_nm(B.RT_SHOP, 'S')    end    AS RT_SHOP_NM, 

         B.RT_SHOP_TYPE, 
         dbo.sf_inter_nm('911', B.RT_SHOP_TYPE) 	AS RT_SHOP_TYPE_NM,
         ISNULL(B.ACCEPT_FG, 'C') 			AS ACCEPT_FG
    FROM TB_54030_H A,
         TB_54031_D B
    WHERE A.YYMMDD   *= B.YYMMDD
     AND A.SHOP_CD  *= B.SHOP_CD
     AND A.RQST_SEQ *= B.RQST_SEQ
     AND A.YYMMDD BETWEEN :as_fr_ymd AND :as_to_ymd
     AND A.SHOP_CD LIKE :as_shop_cd
     AND A.BRAND = :as_brand
     and a.style   like :as_style
     and a.chno    like :as_chno
	  and a.color   like :as_color
     and a.size    like :as_size
" arguments=(("as_brand", string),("as_fr_ymd", string),("as_to_ymd", string),("as_shop_cd", string),("as_style", string),("as_chno", string),("as_color", string),("as_size", string)) )
