 retrieve="  SELECT CAST(:as_work_dt AS VARCHAR(8)) AS WORK_DT, A.CUST_CD AS CUST_CD, 
			A.STYLE AS STYLE, A.CHNO AS CHNO,
			A.STYLE + A.CHNO AS STYLE_NO,
			A.ORD_YMD AS ORDER_YMD, A.DLVY_YMD AS DELIVERY_YMD, 
			ISNULL(SUM(A.ORD_QTY), 0) AS ORDER_QTY, 
			ISNULL(SUM(A.IN_QTY), 0) AS IN_QTY,
			MAX(A.QC_DT) AS QC_DT,
			ISNULL(SUM(A.CUT_QTY), 0)  AS CUT_QTY, 
			ISNULL(SUM(A.SEW_QTY), 0)  AS SEW_QTY,
			ISNULL(SUM(A.FIN_QTY), 0)  AS FIN_QTY, 
			ISNULL(SUM(A.TEST_QTY), 0) AS TEST_QTY,
			ISNULL(SUM(A.CUT_SUM), 0)  AS CUT_SUM, 
			ISNULL(SUM(A.SEW_SUM), 0)  AS SEW_SUM,
			ISNULL(SUM(A.FIN_SUM), 0)  AS FIN_SUM,
			ISNULL(SUM(A.TEST_SUM), 0) AS TEST_SUM,
			ISNULL(SUM(A.CUT_SUM), 0)  - ISNULL(SUM(A.CUT_QTY), 0)  AS CUT_LAST, 
			ISNULL(SUM(A.SEW_SUM), 0)  - ISNULL(SUM(A.SEW_QTY), 0)  AS SEW_LAST, 
			ISNULL(SUM(A.FIN_SUM), 0)  - ISNULL(SUM(A.FIN_QTY), 0)  AS FIN_LAST, 
			ISNULL(SUM(A.TEST_SUM), 0) - ISNULL(SUM(A.TEST_QTY), 0) AS TEST_LAST, 
			MAX(A.REG_ID) AS REG_ID, MAX(A.REG_DT) AS REG_DT,
			MAX(A.MOD_ID) AS MOD_ID, MAX(A.MOD_DT) AS MOD_DT,
			MAX(A.NEW_CHK) AS NEW_CHK
	 FROM ( SELECT A.CUST_CD AS CUST_CD, A.STYLE AS STYLE, A.CHNO AS CHNO,
						A.ORD_YMD AS ORD_YMD, A.DLVY_YMD AS DLVY_YMD, 
						A.ORD_QTY AS ORD_QTY, A.IN_QTY AS IN_QTY,
						ISNULL(B.QC_DT, A.QC_YMD) AS QC_DT,
						B.CUT_QTY AS CUT_QTY, B.SEW_QTY AS SEW_QTY,
						B.FIN_QTY AS FIN_QTY, B.TEST_QTY AS TEST_QTY,
						0 AS CUT_SUM, 0 AS SEW_SUM, 0 AS FIN_SUM, 0 AS TEST_SUM,
						B.REG_ID AS REG_ID, B.REG_DT AS REG_DT,
						B.MOD_ID AS MOD_ID, B.MOD_DT AS MOD_DT,
						ISNULL(B.STYLE, 'N') AS NEW_CHK
				 FROM	( SELECT A.CUST_CD AS CUST_CD, A.STYLE AS STYLE, A.CHNO AS CHNO,
									A.ORD_YMD AS ORD_YMD, A.DLVY_YMD AS DLVY_YMD, A.QC_YMD AS QC_YMD,
									SUM(A.ORD_QTY) AS ORD_QTY, SUM(A.IN_QTY) AS IN_QTY
							 FROM ( SELECT A.CUST_CD AS CUST_CD, A.STYLE AS STYLE, A.CHNO AS CHNO,
												A.ORD_YMD AS ORD_YMD, A.DLVY_YMD AS DLVY_YMD, A.QC_YMD AS QC_YMD,
												SUM(B.ORD_QTY) AS ORD_QTY, 0 AS IN_QTY
										 FROM TB_12021_D A,
												TB_12030_S B
										WHERE A.STYLE = B.STYLE
										  AND A.CHNO  = B.CHNO
										  AND A.CUST_CD = :as_cust_cd
										  AND A.YEAR BETWEEN :as_st_yy AND :as_ed_yy
										  AND ISNULL(A.LAST_YMD, '99999999') > :as_work_dt
										GROUP BY A.CUST_CD, A.STYLE, A.CHNO, A.ORD_YMD, A.DLVY_YMD, A.QC_YMD
										UNION ALL
									  SELECT A.CUST_CD AS CUST_CD, A.STYLE AS STYLE, A.CHNO AS CHNO,
												A.ORD_YMD AS ORD_YMD, A.DLVY_YMD AS DLVY_YMD, A.QC_YMD AS QC_YMD,
												0 AS ORD_QTY, SUM(B.IN_QTY) AS IN_QTY
										 FROM TB_12021_D A,
												TB_12031_S B
										WHERE A.STYLE = B.STYLE
										  AND A.CHNO  = B.CHNO
										  AND A.CUST_CD = :as_cust_cd
										  AND B.YEAR BETWEEN :as_st_yy AND :as_ed_yy
										  AND B.YYMM <= CONVERT(VARCHAR(6), DATEADD(MM, -1, CAST(:as_work_dt AS DATETIME)), 112)
										  AND ISNULL(A.LAST_YMD, '99999999') > :as_work_dt
										GROUP BY A.CUST_CD, A.STYLE, A.CHNO, A.ORD_YMD, A.DLVY_YMD, A.QC_YMD
										UNION ALL
									  SELECT A.CUST_CD AS CUST_CD, A.STYLE AS STYLE, A.CHNO AS CHNO,
												A.ORD_YMD AS ORD_YMD, A.DLVY_YMD AS DLVY_YMD, A.QC_YMD AS QC_YMD,
												0 AS ORD_QTY, SUM(B.IN_QTY) AS IN_QTY
										 FROM TB_12021_D A,
												TB_12032_S B
										WHERE A.STYLE = B.STYLE
										  AND A.CHNO  = B.CHNO
										  AND A.CUST_CD = :as_cust_cd
										  AND B.YEAR BETWEEN :as_st_yy AND :as_ed_yy
										  AND B.YYMMDD BETWEEN LEFT(:as_work_dt, 6) + '01' AND :as_work_dt
										  AND ISNULL(A.LAST_YMD, '99999999') > :as_work_dt
										GROUP BY A.CUST_CD, A.STYLE, A.CHNO, A.ORD_YMD, A.DLVY_YMD, A.QC_YMD 
                           ) A
							GROUP BY A.CUST_CD, A.STYLE, A.CHNO, A.ORD_YMD, A.DLVY_YMD, A.QC_YMD 
                  ) A,
						TB_31011_H B
				WHERE A.CUST_CD *= B.CUST_CD 
				  AND A.STYLE   *= B.STYLE 
				  AND A.CHNO    *= B.CHNO
				  AND B.WORK_DT = :as_work_dt
				UNION ALL
			  SELECT A.CUST_CD AS CUST_CD, A.STYLE AS STYLE, A.CHNO AS CHNO,
						A.ORD_YMD AS ORD_YMD, A.DLVY_YMD AS DLVY_YMD, 
						0 AS ORD_QTY, 0 AS IN_QTY,
						NULL AS QC_DT,
						0 AS CUT_QTY, 0 AS SEW_QTY, 0 AS FIN_QTY, 0 AS TEST_QTY,
						SUM(B.CUT_QTY) AS CUT_SUM, SUM(B.SEW_QTY)  AS SEW_SUM,
						SUM(B.FIN_QTY) AS FIN_SUM, SUM(B.TEST_QTY) AS TEST_SUM,
						NULL AS REG_ID, NULL AS REG_DT,
						NULL AS MOD_ID, NULL AS MOD_DT,
						NULL AS NEW_CHK
				 FROM VI_12020_1 A, 
						TB_31011_H B
				WHERE A.STYLE   = B.STYLE 
				  AND A.CHNO    = B.CHNO
				  AND A.CUST_CD = B.CUST_CD 
				  AND A.CUST_CD = :as_cust_cd
				  AND A.YEAR BETWEEN :as_st_yy AND :as_ed_yy
				  AND ISNULL(A.LAST_YMD, '99999999') > :as_work_dt
				  AND B.WORK_DT <= :as_work_dt
				GROUP BY A.CUST_CD, A.STYLE, A.CHNO, A.ORD_YMD, A.DLVY_YMD 
         ) A
	GROUP BY A.CUST_CD, A.STYLE, A.CHNO, A.ORD_YMD, A.DLVY_YMD
" update="TB_31011_H" updatewhere=0 updatekeyinplace=yes arguments=(("as_work_dt", string),("as_cust_cd", string),("as_st_yy", string),("as_ed_yy", string)) )
