 column=(type=char(8) update=yes updatewhereclause=yes name=make_ymd dbname="make_ymd" )
 retrieve="  SELECT B.STYLE,   B.CHNO,   
         B.MAT_CD,   B.MAT_NM,   
         B.CUST_CD,   dbo.sf_cust_nm(B.CUST_CD, 'S') AS CUST_NM, 
         B.MAKE_PRICE,   B.ETC_PRICE,   
         B.REQ_YMD,   B.ORD_YMD,   
         (select top 1 yymmdd from tb_12029_tl (nolock)
					where style = b.style
					and   chno  = b.chno
					and   tl_gbn = '1'
					order by yymmdd)	as tag_ymd,  			
         B.FIX_DLVY,   B.DLVY_YMD,   B.LAST_YMD,   
         ISNULL(B.IWOL_MARK, '0') AS IWOL_MARK,   
         B.IWOL_YMD,   B.REMARK,   
         B.REG_ID,   B.REG_DT,   B.MOD_ID,   B.MOD_DT,   
         A.BRAND,   dbo.sf_inter_nm('001', A.BRAND) AS BRAND_NM,
         A.YEAR,   
         A.SEASON,   dbo.sf_inter_nm1('003', A.SEASON, a.brand, a.year) AS SEASON_NM,
         A.SOJAE,   dbo.sf_sojae_nm4(A.SOJAE,a.brand, a.year, a.season) AS SOJAE_NM,
         A.ITEM,   dbo.sf_item_nm4(A.ITEM, a.brand, a.year, a.season) AS ITEM_NM,
         b.OUT_SEQ,   dbo.sf_inter_nm('010', b.OUT_SEQ) AS OUT_SEQ_NM, 
         A.SAMPLE_CD,   
         isnull(b.make_type,A.MAKE_TYPE) make_type,   dbo.sf_inter_nm('030', isnull(b.make_type,A.MAKE_TYPE)) AS MAKE_TYPE_NM, 
         A.PATT_TYPE,   dbo.sf_inter_nm('020', A.PATT_TYPE) AS PATT_TYPE_NM, 
         isnull(b.orgmat_cd,A.ORGMAT_CD) orgmat_cd,   dbo.sf_inter_nm('021', isnull(b.orgmat_cd,A.ORGMAT_CD)) AS ORGMAT_NM, 
         A.DSGN_EMP,   dbo.sf_emp_nm(A.DSGN_EMP)   AS DSGN_EMP_NM, 
         A.CONCEPT,   dbo.sf_inter_nm('122', A.CONCEPT) AS CONCEPT_NM, 
         A.PLAN_YN,   
         isnull(b.country_cd,A.COUNTRY_CD) country_cd,   dbo.sf_inter_nm('000', isnull(b.country_cd, isnull(A.COUNTRY_CD,'00'))) AS COUNTRY_NM, 
         A.TAG_PRICE,   
         A.STYLE_INFO ,
		   (select min(in_ymd) from tb_12030_s where style = b.style and chno = b.chno and year = b.year and season = b.season) fin_ymd,
			b.cancel_yn,
			b.pay_gubn,
			case when exists (select top 1 * from tb_41010_h (nolock)
			where style = b.style
			and   chno  = b.chno
			and year = b.year 
			and season = b.season
			and   bill_date is not null) then 1 else 0 end  as make_price_protect,
			(select max(isnull(note, dbo.sf_inter_nm('211',claim_gubn) )) from tb_23010_m (nolock) where style = b.style and chno = b.chno and st_year = b.year and st_season = b.season) 	claim_note,
			a.smat_info,
			a.fabric_by,
			b.factory_cd,
			isnull(b.design_agree,'N') as design_agree,
			b.make_ymd	
				
    FROM TB_12020_M  A(nolock),   
         TB_12021_D  B(nolock) 
   WHERE B.STYLE  = A.STYLE 
	  and A.STYLE = :as_style
     AND B.CHNO  = :as_chno

" update="TB_12021_D" updatewhere=0 updatekeyinplace=yes arguments=(("as_style", string),("as_chno", string)) )
