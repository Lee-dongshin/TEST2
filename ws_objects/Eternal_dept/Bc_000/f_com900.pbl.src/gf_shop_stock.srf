SELECT dbo.sf_shop_stock(:as_shop_cd, :as_shop_type, :as_style, :as_chno, :as_color, :as_size) 
  INTO :al_stock_qty
  FROM DUAL ;


IF ISNULL(al_stock_qty) THEN RETURN 100

RETURN sqlca.sqlcode 

end function

