table(column=(type=char(4) updatewhereclause=yes name=mat_sojae dbname="mat_sojae" )
 column=(type=char(43) updatewhereclause=yes name=mat_sojae_nm dbname="mat_sojae_nm" )
 retrieve="  select sojae    as mat_sojae, 
			case when :as_brand in ('B') then  SOJAE_wNM + ' | ' + sojae_xnm
				  when :as_brand in ('K','L','P') then  SOJAE_xNM 
				  when :as_brand in ('U')	 then SOJAE_ZNM
				  when :as_brand in ('G')	 then SOJAE_YNM
			else SOJAE_NM end as mat_sojae_nm
    from tb_91020_c  with (nolock)
   where '1'      = :as_mat_gubn
    and  isnull(case when :as_brand in ('K','L','P') then  SOJAE_xNM  when :as_brand in ('U') then SOJAE_ZNM when :as_brand in ('G') then SOJAE_YNM when :as_brand in ('B') then SOJAE_wNM + ' | ' + sojae_xnm else SOJAE_NM end,'') <> '' 
  union all
  select inter_cd as mat_sojae, 
         inter_nm as mat_sojae_nm 
    from tb_91011_c  with (nolock)
   where inter_grp = '006' 
     and '2'       = :as_mat_gubn 

" arguments=(("as_mat_gubn", string),("as_brand", string)) )
