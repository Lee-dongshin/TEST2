 retrieve="  SELECT a.Color                       AS Color,   
         dbo.sf_color_nm(a.Color, 'E') AS Color_Nm, 
         a.Size                        AS Size,   
         dbo.sf_size_nm(a.Size)        AS Size_Nm, 
         b.Plan_Qty                    AS plan_qty,   
         b.Ord_Qty                     AS order_qty,   
         b.in_Qty                      as in_qty,   
         b.Bad_Qty                     as bad_qty,   
         b.iwol_Qty                    as iwol_qty,   
         b.sale_qty                    as sale_qty, 
         b.etc_qty                     as etc_qty 
    FROM TB_12024_D A, 
         TB_12030_S B   
   WHERE A.Style = B.Style 
     And a.chno  = b.chno
     and a.color = b.color
     and a.size  = b.size 
     AND (A.Style = :as_style
      AND A.Chno  = :as_chno ) " arguments=(("as_style", string),("as_chno", string)) )
