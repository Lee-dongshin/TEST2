 retrieve="  select sum(plan_amt)  / 1000   as plan_amt, 
         sum(case when sale_div = '1' then plan_amt end) / 1000 plan_amt_1,
         sum(case when sale_div = '2' then plan_amt end) / 1000 plan_amt_2,
         sum(case when sale_div = '3' then plan_amt end) / 1000 plan_amt_3,
         sum(case when sale_div = '4' then plan_amt end) / 1000 plan_amt_4,
         0   rate_1, 
         0   rate_2, 
         0   rate_3, 
         0   rate_4 
    from vi_11040_1 
   where brand  = :as_brand 
     and year   = :as_year 
     and season = :as_season
	  and last_yn= 'Y' 
" arguments=(("as_brand", string),("as_year", string),("as_season", string)) )
