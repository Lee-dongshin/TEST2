 retrieve="  SELECT a.style,   
         a.color,   
         a.chno,   
         a.size,     
         a.event_gubn  
    from tb_54060_h a with(nolock) left outer join vi_12024_1 b with(nolock) on a.style = b.style and a.chno = b.chno and a.color = b.color and a.size = b.size
	where b.brand like :is_brand
	  and b.year like :is_year
	  and b.season like :is_season
	  and b.style like :is_style
	  and b.chno like :is_chno
	  and b.color like :is_color
	  and a.event_gubn like :is_event_gubn" arguments=(("is_brand", string),("is_year", string),("is_season", string),("is_style", string),("is_chno", string),("is_color", string),("is_event_gubn", string)) )
