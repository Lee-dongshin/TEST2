table(column=(type=char(20) updatewhereclause=yes name=shop_knm dbname="shop_knm" )
 column=(type=number updatewhereclause=yes name=sale_amt dbname="sale_amt" )
 retrieve=" select 	substring(a.yymmdd,7,2)		as day,	
	b.shop_knm,
	sum(isnull(a.sale_amt,0)) 	as sale_amt
 from  (select  	a.yymmdd,
		a.shop_cd		as shop_cd, 
		a.style			as style,	
		sum(isnull(a.sale_qty,0)) 	as sale_qty,
		sum(isnull(a.sale_amt,0)) 	as sale_amt
	 from 	china.dbo.tb_53010_h   a
   	where  a.yymmdd  between  :as_fr_yymmdd and :as_to_yymmdd
  	 and	a.brand  = 'N'
	 group by a.yymmdd, a.shop_cd,  a.style) a,
             china.dbo.tb_91100_M b
 where  a.shop_cd = b.shop_cd
 group by a.shop_cd,b.shop_knm, substring(a.yymmdd,7,2)
 having   sum(isnull(a.sale_amt,0)) <> 0
 order by  a.shop_cd, substring(a.yymmdd,7,2)" arguments=(("as_fr_yymmdd", string),("as_to_yymmdd", string))  sort="shop_knm A " )
