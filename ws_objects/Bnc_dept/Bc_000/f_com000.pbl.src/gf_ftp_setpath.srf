global function integer gf_ftp_setpath (string ls_remotepath);ulong ul_set

ul_set = u_ftp.FtpSetCurrentDirectoryA(ll_connect, ls_remotepath)

if ul_set = 0 then
	return -1
else
	return 1
end if
end function

