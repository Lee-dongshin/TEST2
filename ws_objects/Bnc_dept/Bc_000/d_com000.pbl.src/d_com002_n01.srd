 retrieve="  SELECT SOJAE,
			case when :as_brand in ('B','L','F') then  isnull(SOJAE_wNM,'') + ' | ' + isnull(sojae_xnm,'')
				  when :as_brand in ('K','P') then  SOJAE_xNM 
				  when :as_brand in ('xU')	 then SOJAE_ZNM
				  when :as_brand in ('G')	 then SOJAE_YNM
				  when :as_brand in ('D')	 then SOJAE_VNM
			else SOJAE_NM end as sojae_nm,
			SOJAE_GRP
	 FROM TB_91020_C
	WHERE ISNULL(SOJAE_GRP, '') LIKE :as_sojae_grp
    and  isnull(case when :as_brand in ('K') then  SOJAE_xNM  when :as_brand in ('xU') then SOJAE_ZNM when :as_brand in ('G') then SOJAE_YNM when :as_brand in ('B','L','F') then isnull(SOJAE_xNM,sojae_wnm) else SOJAE_NM end,'') <> ''
	 and  isnull( case when :as_brand in ('B','L','F') then isnull(SOJAE_wNM,'') + ' | ' + isnull(sojae_xnm,'') else sojae_nm end ,'') <> ' | '" update="TB_91020_C" updatewhere=0 updatekeyinplace=no arguments=(("as_sojae_grp", string),("as_brand", string)) )
