table(column=(type=char(255) updatewhereclause=yes name=shop_nm dbname="shop_nm" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_qty dbname="sale_qty" )
 retrieve="select top 10  replace(replace(dbo.sf_shop_nm(shop_cd, 's'),'(올리브)',''),'(W.)','') as shop_nm, 
  sum(isnull(sale_qty,0)) - sum(isnull(sale_qty9,0)) as sale_qty
from tb_44012_s a (nolock), tb_12020_m b (nolock)
where a.style    = :As_style
and   a.style    = b.style
and   a.year     = b.year 
and   a.season   = b.season
and   a.shop_div in ('G','K','D','B','E','H')
group by shop_cd
having  sum(isnull(sale_qty,0)) - sum(isnull(sale_qty9,0))  > 0
order by 2 desc
" arguments=(("as_style", string)) )
