 column=(type=char(14) updatewhereclause=yes name=display dbname="display" )
 retrieve="SELECT DISTINCT datepart(hour, A.mod_dt) AS INPUT_TIME,
		 YYMMDD,
 		 right(convert(char(04), 1000 + datepart(hour, A.mod_dt)),2) + '시(24시간)' as display
	from  tb_45052_h a (NOLOCK)
	where yymmdd = CONVERT(CHAR(08), GETDATE(), 112)
union 
SELECT DISTINCT datepart(hour, A.mod_dt) AS INPUT_TIME,
		 YYMMDD,
 		 right(convert(char(04), 1000 + datepart(hour, A.mod_dt)),2) + '시(24시간)' as display
	from  tb_45050_h a (NOLOCK)
	where yymmdd = CONVERT(CHAR(08), GETDATE(), 112)
UNION 
SELECT  0, CONVERT(CHAR(08), GETDATE(), 112), '금일분전체' 
ORDER BY 1" )
