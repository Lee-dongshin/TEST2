 retrieve="  SELECT A.STYLE, A.CHNO,
         ISNULL(A.MMAT_PRICE, 0) AS MMAT_PRICE,
         ISNULL(A.SMAT_PRICE, 0) AS SMAT_PRICE,
         ISNULL(A.MAKE_PRICE, 0) AS MAKE_PRICE,
         ISNULL(A.ETC_PRICE, 0) AS ETC_PRICE,
         ISNULL(B.BUSI_PRICE, 0) AS BUSI_PRICE,
         ISNULL(B.MD_PRICE, 0) AS MD_PRICE,
         ISNULL(B.TAG_PRICE, 0) AS TAG_PRICE,
         CASE WHEN B.BRAND = 'o' then c.inter_data2 else C.INTER_DATA1 end AS BAE_SU,
			space(2)		as	SEASON_BAE_SU,
         A.MOD_ID, A.MOD_DT,
			isnull(a.tag_agree,'N') as tag_agree,
			a.wonga_price,
         ISNULL(A.manuf_PRICE, 0) AS manuf_PRICE
    FROM TB_12021_D A,
         TB_12020_M B,
         TB_91011_C C
   WHERE A.STYLE     = B.STYLE
     AND B.MAKE_TYPE = C.INTER_CD
     AND A.STYLE = LEFT(:as_style_no, 8)
     AND A.CHNO  = SUBSTRING(:as_style_no, 9, 1)
     AND C.INTER_GRP = '030'
" update="TB_12021_D" updatewhere=0 updatekeyinplace=yes arguments=(("as_style_no", string)) )
