 retrieve="  SELECT a.STYLE, a.CHNO, a.SUB_JOB,
         a.CUST_CD, dbo.sf_cust_nm(a.CUST_CD, 'S') AS CUST_NM,
         a.ORD_YMD, a.DLVY_YMD, a.PRICE,
			a.BRAND, a.YEAR, a.SEASON, a.SOJAE, a.ITEM,
         a.REG_ID, a.REG_DT, a.MOD_ID, a.MOD_DT,
			isnull(b.design_agree,'N')	as design_agree
    FROM TB_12026_D a(nolock), tb_12021_d b(nolock)
   WHERE a.style = b.STYLE 
	and   a.chno  = b.chno
	and   a.style = :as_style
   and   a.chno  = :as_chno
" update="TB_12026_D" updatewhere=0 updatekeyinplace=yes arguments=(("as_style", string),("as_chno", string)) )
