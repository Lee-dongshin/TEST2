 retrieve="SELECT A.slip_bonji,
       A.brand,
       A.fore_ymd,   
       A.fore_no,   
       A.cust_cd,
       B.cust_sname AS cust_nm,
       A.rmk,   
       A.io_gubn,   
       A.fore_amt, 
       0 AS fore_jan_amt,  
       A.slip_date,  
       A.slip_no,   
       A.reg_id,   
       A.reg_dt,   
       A.mod_id,   
       A.mod_dt,
		 A.mat_type  
  FROM TB_33010_H A, MIS.DBO.TSB04 B 
 WHERE A.brand      = :as_brand 
   AND A.fore_ymd   = :as_fore_ymd  
   AND A.fore_no LIKE :as_fore_no + '%' 
   AND A.cust_cd LIKE :as_cust_cd + '%'   
   AND A.cust_cd   *= B.CUSTCODE" update="tb_33010_h" updatewhere=0 updatekeyinplace=no arguments=(("as_brand", string),("as_fore_ymd", string),("as_fore_no", string),("as_cust_cd", string)) )
