 retrieve="SELECT YYMMDD,
		 SEQ_NO,
		 PERSON_NM
FROM TB_79023_D (NOLOCK)
WHERE BRAND  							LIKE :AS_BRAND + '%'
and   shop_cd 							LIKE :as_shop_cd + '%'
and	YYMMDD BETWEEN :AS_FR_YMD AND :AS_TO_YMD
AND   PERSON_NM    					LIKE :AS_PERSON_NM + '%'
AND 	isnull(COUNSEL_EMPNO,'%')	LIKE :AS_EMPNO + '%'
AND   isnull(COUNSEL_TYPE,'%')	LIKE :AS_COUNSEL_TYPE +'%'
AND	isnull(INVOICE_NO,'%')   	LIKE :AS_INVOICE_NO + '%'
AND   isnull(TEL_NO1,'%') 			LIKE :AS_TEL1 + '%'  
AND   isnull(TEL_NO2,'%') 			LIKE :AS_TEL2 + '%'
" update="TB_79023_D" updatewhere=1 updatekeyinplace=no arguments=(("AS_BRAND", string),("as_shop_cd", string),("AS_FR_YMD", string),("AS_TO_YMD", string),("AS_PERSON_NM", string),("AS_EMPNO", string),("AS_COUNSEL_TYPE", string),("AS_INVOICE_NO", string),("AS_TEL1", string),("AS_TEL2", string))  sort="yymmdd D person_nm A " )
