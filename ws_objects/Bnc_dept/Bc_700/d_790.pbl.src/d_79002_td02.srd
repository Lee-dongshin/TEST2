 retrieve="  SELECT B.YYMMDD, B.SEQ_NO, B.NO,
         A.JUMIN, A.CARD_NO, A.CUSTOM_NM,
         A.SEX, A.TEL_NO1, A.TEL_NO2, A.TEL_NO3, 
         A.SHOP_CD, dbo.sf_shop_nm(A.SHOP_CD, 'S') AS SHOP_NM, 
         A.RCV_HOW, A.WANT_YMD, 
         B.STYLE, B.CHNO, 
         C.CUST_CD AS ST_CUST_CD,  dbo.sf_cust_nm(C.CUST_CD, 'S') AS ST_CUST_NM,
         D.MAT_CD, D.MAT_NM,
         D.CUST_CD AS MAT_CUST_CD, dbo.sf_cust_nm(D.CUST_CD, 'S') AS MAT_CUST_NM,
         B.COLOR, dbo.sf_color_nm(B.COLOR, 'E') AS COLOR_NM,
         B.SIZE,  dbo.sf_size_nm (B.SIZE      ) AS SIZE_NM,
         B.SALE_YMD, B.RCV_SPOT, B.WASH_FG, B.WASH_NO, B.HAN_MARK,
         B.RCV_WHY, B.RCV_REQ, B.PROBLEM, B.JUDG_FG, B.JUDG_YMD, 
         B.BRAND, B.REG_ID, B.REG_DT, B.MOD_ID, B.MOD_DT
    FROM TB_79010_M A,
         TB_79011_D B,
         VI_12020_1 C,
         TB_21010_M D
   WHERE A.YYMMDD = B.YYMMDD
     AND A.SEQ_NO = B.SEQ_NO
     AND B.STYLE  = C.STYLE
     AND B.CHNO   = C.CHNO
     AND C.MAT_CD = D.MAT_CD
     AND A.YYMMDD = :as_yymmdd
     AND A.SEQ_NO = :as_seq_no
" update="TB_79011_D" updatewhere=0 updatekeyinplace=yes arguments=(("as_yymmdd", string),("as_seq_no", string)) )
