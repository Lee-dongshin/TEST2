 retrieve="  SELECT ISNULL(C.JUDG_FG, '0') AS JUDG_FG, 
         dbo.sf_inter_nm('799', ISNULL(C.JUDG_FG, '0') ) AS JUDG_FG_NM,
         C.YYMMDD,  B.STYLE + B.CHNO AS STYLE_NO, 
         A.SHOP_CD, dbo.sf_shop_nm (A.SHOP_CD, 'S')   AS SHOP_NM,
         C.CUST_CD, dbo.sf_cust_nm (C.CUST_CD, 'S')   AS CUST_NM,
         B.RCV_WHY, dbo.sf_inter_nm('793', B.RCV_WHY) AS RCV_WHY_NM,
         B.RCV_REQ, dbo.sf_inter_nm('794', B.RCV_REQ) AS RCV_REQ_NM,
         REPLACE(B.PROBLEM, CHAR(13) + CHAR(10), '   ') AS PROBLEM, 
         C.ED_YMD, 
         C.DEAL_FG, dbo.sf_inter_nm('798', C.DEAL_FG) AS DEAL_FG_NM,
         REPLACE(C.RESULT, CHAR(13) + CHAR(10), '   ') AS RESULT, 
         REPLACE(C.REMARK, CHAR(13) + CHAR(10), '   ') AS REMARK
    FROM TB_79010_M A,
         TB_79011_D B,
         TB_79020_H C
   WHERE A.YYMMDD = B.YYMMDD
     AND A.SEQ_NO = B.SEQ_NO
     AND B.YYMMDD = C.YYMMDD
     AND B.SEQ_NO = C.SEQ_NO
     AND B.NO     = C.NO
     AND C.YYMMDD BETWEEN :as_fr_ymd AND :as_to_ymd
     AND ISNULL(C.DEPT_CD, '0000') LIKE :as_dept_cd
     AND C.BRAND = :as_brand
     AND ISNULL(C.JUDG_FG, '0') IN (:as_judg_fg1, :as_judg_fg2)
     AND ISNULL(C.DEAL_FG, '00') LIKE :as_deal_fg
" arguments=(("as_brand", string),("as_fr_ymd", string),("as_to_ymd", string),("as_dept_cd", string),("as_judg_fg1", string),("as_judg_fg2", string),("as_deal_fg", string)) )
