 column=(type=char(30) updatewhereclause=yes name=shop_nm dbname="SHOP_NM" )
 retrieve="  SELECT A.YYMMDD, A.SEQ_NO, B.NO, A.JUMIN, A.CARD_NO, A.CUSTOM_NM, 
         B.JUDG_FG, dbo.sf_inter_nm('799', B.JUDG_FG) AS JUDG_FG_NM,
         B.JUDG_YMD, A.SHOP_CD, dbo.sf_shop_nm(A.SHOP_CD, 'S') AS SHOP_NM
    FROM TB_79010_M A with (nolock),
         TB_79011_D B with (nolock)
   WHERE A.YYMMDD = B.YYMMDD
     AND A.SEQ_NO = B.SEQ_NO
     AND A.YYMMDD    BETWEEN :as_fr_ymd AND :as_to_ymd
     AND A.JUMIN     LIKE    :as_jumin
     AND A.CARD_NO   LIKE    :as_card_no
     AND A.CUSTOM_NM LIKE    :as_custom_nm
     AND A.BRAND     =       :as_brand
     AND ISNULL(B.JUDG_FG, '0') LIKE :as_judg_fg
" arguments=(("as_brand", string),("as_fr_ymd", string),("as_to_ymd", string),("as_judg_fg", string),("as_card_no", string),("as_jumin", string),("as_custom_nm", string)) )
