 retrieve="select 	a.yymmdd,
			a.seq_no,
			a.custom_nm,
			a.shop_cd,
			a.brand,
			case	when isnull(a.shop_cd,'') = '' then dbo.sf_inter_nm('001',a.brand) + ' 고객상담팀'
					when isnull(a.shop_cd,'') <> '' then dbo.sf_shop_nm(a.SHOP_CD, 'S')
			else '' end  AS shop_nm,
			b.style,
			b.problem,
			case	when isnull(b.pay_fg,'') = ''  then '무상'
					when isnull(b.pay_fg,'') = '1' then '무상'
					when isnull(b.pay_fg,'') = '2' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '3' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '4' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '5' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '5' and isnull(b.pay_amt,0) > 0 then convert(varchar(12),b.pay_amt)
					when isnull(b.pay_fg,'') = '6' and isnull(b.pay_amt,0) = 0 then '무상'
					when isnull(b.pay_fg,'') = '6' and isnull(b.pay_amt,0) > 0 then convert(varchar(12),b.pay_kamt)
			else '' end as amt
			
from		tb_79010_m a with (nolock) inner join tb_79011_d b with (nolock) on a.yymmdd = b.yymmdd and a.seq_no = b.seq_no
where		a.yymmdd = :as_yymmdd and
			a.seq_no = :as_seq_no" arguments=(("as_yymmdd", string),("as_seq_no", string)) )
