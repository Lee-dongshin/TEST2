 retrieve="

select a.reserv_date, a.reserv_time, a.reserv_text, 
       b.CUSTOM_NM, 
       b.SHOP_CD, dbo.sf_shop_nm(b.SHOP_CD, 'S') AS SHOP_NM,
       b.tel_no3, b.yymmdd, b.seq_no, a.reserv_complete
	
		
from tb_79014_d a (nolock), tb_79010_m b (nolock)
where a.reserv_date = convert(char(8), getdate(), 112)
and a.yymmdd = b.yymmdd
and a.seq_no = b.seq_no
and a.reg_id like :as_empno + '%'
and a.reserv_time between  replace( convert(char(5), dateadd(mi,0, getdate()), 108), ':','')  and  replace( convert(char(5), dateadd(mi,0, getdate()), 108), ':','')
and isnull(a.reserv_complete,'N') = 'N'
" arguments=(("as_empno", string)) )
