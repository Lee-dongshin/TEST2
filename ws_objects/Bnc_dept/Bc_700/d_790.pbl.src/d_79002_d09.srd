 retrieve="select	a.brand,
	DBO.SF_INTER_NM('001', a.BRAND) 	AS BRAND_NM,
	DBO.SF_INTER_SORT_SEQ('001', a.BRAND) AS BRAND_SORT,
	a.yymmdd,
	a.seq_no,
	a.no,
	a.style,
	custom_nm,
	a.remark

from TB_79011_D a with (nolock) left outer join tb_79010_m b with (nolock) on a.yymmdd = b.yymmdd and a.seq_no = b.seq_no
where isnull(a.judg_L,'') = '' and 
		a.brand = :as_brand and 
		a.yymmdd between :ps_fr_yymm + '01' and :ps_to_yymm+'31' and
		a.year like :as_year + '%' and
		a.season like :as_season + '%'
order by a.brand_sort, a.yymmdd
" arguments=(("as_brand", string),("ps_fr_yymm", string),("ps_to_yymm", string),("as_year", string),("as_season", string)) )
