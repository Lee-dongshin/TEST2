		SELECT A.SHOP_CD,
			a.goods_amt /10 AS GIVE_POINT,
			sum(A.sale_qty) as sale_qty         
		 FROM TB_71012_H  a with (nolock)
		 where a.sale_ymd between '20030818' and '20030831'
		 and   left(isnull(a.coupon_no,''),1) in ('N', 'O')
		 group by A.SHOP_CD, a.goods_amt
 "  sort="shop_cd A shop_cd A " )
