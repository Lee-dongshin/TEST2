 procedure="
	SELECT A.SHOP_CD, DBO.SF_SHOP_NM(A.SHOP_CD,'S') AS SHOP_NM, A.GIVE_POINT, A.SALE_QTY AS GIVE_QTY, B.SALE_QTY AS ACCEPT_QTY
	FROM	(SELECT  M.SHOP_CD, M.GIVE_POINT, SUM(M.SALE_QTY) AS SALE_QTY 
		FROM	(SELECT H.JUMIN,(SELECT SHOP_CD FROM TB_71010_M WHERE JUMIN = H.JUMIN) AS SHOP_CD, 
		 		H.GIVE_POINT, COUNT(H.GIVE_POINT) AS SALE_QTY
			 FROM   TB_71011_H H WITH (NOLOCK)
			 WHERE  H.GIVE_DATE = '20030818' 
			  AND   H.POINT_FLAG = '1'
			group by  H.JUMIN,H.GIVE_POINT) M
		GROUP BY M.SHOP_CD,M.GIVE_POINT) A ,
		(SELECT A.SHOP_CD,
			a.goods_amt /10 AS GIVE_POINT,
			sum(A.sale_qty) as sale_qty         
		 FROM TB_71012_H  a with (nolock)
		 where a.sale_ymd between '20030818' and '20030831'
		 and   left(isnull(a.coupon_no,''),1) in ('N', 'O')
		 group by A.SHOP_CD, a.goods_amt) B
         WHERE   A.SHOP_CD = B.SHOP_CD
	AND      A.GIVE_POINT = B.GIVE_POINT"  sort="shop_cd A shop_nm A " )
