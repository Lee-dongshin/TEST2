 retrieve="  SELECT A.style,   
         B.chno,   
         B.mat_cd,   
         B.mat_nm,   
         B.cust_cd,   
         dbo.sf_cust_nm(b.cust_cd, 'S') as cust_nm, 
         B.make_price,   
         B.etc_price,   
         B.req_ymd,   
         B.ord_ymd,   
         (select top 1 yymmdd from tb_12029_tl (nolock)
					where style = b.style
					and   chno  = b.chno
					and   tl_gbn = '1'
					order by yymmdd)	as tag_ymd,   
         B.fix_dlvy,   
         B.dlvy_ymd,   
         B.last_ymd,   
         B.iwol_mark,   
         B.iwol_ymd,   
         B.remark,   
         A.reg_id,   
         A.reg_dt,   
         A.mod_id,   
         A.mod_dt,   
         A.brand,   
         A.year,   
         A.season,   
         A.sojae,   
         A.item,   
         b.out_seq,   
         isnull(a.sample_cd, (select sample_cd from tb_31031_m (nolock) where style = a.style) )	as sample_cd ,   
			A.make_type,
         b.make_type as make_type2,   
         A.patt_type,   
         A.orgmat_cd,
			b.orgmat_cd	as orgmat_cd2,   
         A.dsgn_emp,   
         dbo.sf_emp_nm(a.dsgn_emp)   as dsgn_emp_nm, 
         A.concept,   
         A.plan_yn,   
         A.country_cd,
			isnull(b.country_cd,'00') as country_cd2,   
         A.tag_price,   
         A.style_info,
			b.cancel_yn,
			b.pay_gubn,

			case when exists(select top 1 * from tb_12030_s (nolock) where style = b.style and chno = b.chno and isnull(in_qty,0) <> 0) then 1 else 0 end as prot,
		   (select out_seq from tb_12028_d (nolock) where style = b.style and chno = b.chno)	as out_seq2,
			a.style_type,
			a.washing_yn,
			a.mat_use_way,
			a.mat_direction,
			(select max(isnull(note, dbo.sf_inter_nm('211',claim_gubn) )) from tb_23010_m (nolock) where style = b.style and chno = b.chno AND ST_YEAR = B.YEAR) 	as claim_note,
			a.smat_info	as smat_info,
			coalesce(a.fabric_by, dbo.SF_mat_country(a.style,b.chno)) as fabric_by,
			a.fz_code,
			b.factory_cd,
			b.zsojae,
			b.zitem,
			a.trial_sheet,
			(select maker_fg from tb_12029_w (nolock)
					where style = b.style
					and   chno  = b.chno)	as maker_fg,
			b.dlvy_ymd_shop,
         (select top 1 yymmdd from tb_12029_tl (nolock)
					where style = b.style
					and   chno  = b.chno
					and   tl_gbn = '3'
					order by yymmdd)	as label_ymd,
			a.style_info2,
			a.size_gubn,
			b.out_seq_chn,
		   b.make_ymd,
			b.exp_ymd,
			b.cost_year,
			a.line_nm
    FROM tb_12020_m  A(nolock),   
         tb_12021_d  B(nolock) 
   WHERE B.style  =* A.style 
     AND B.chno  = :as_chno
     AND (A.style = :as_style) 
" update="tb_12020_m" updatewhere=0 updatekeyinplace=yes arguments=(("as_style", string),("as_chno", string)) )
