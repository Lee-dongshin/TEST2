 retrieve="  SELECT A.style,   
         B.chno,   
         B.mat_cd,   
         B.mat_nm,   
         B.cust_cd,   
         dbo.sf_cust_nm(b.cust_cd, 'S') as cust_nm, 
         B.make_price,   
         B.etc_price,   
         B.req_ymd,   
         B.ord_ymd,   
         B.tag_ymd,   
         B.fix_dlvy,   
         B.dlvy_ymd,   
         B.last_ymd,   
         B.iwol_mark,   
         B.iwol_ymd,   
         B.remark,   
         A.reg_id,   
         A.reg_dt,   
         A.mod_id,   
         A.mod_dt,   
         A.brand,   
         A.year,   
         A.season,   
         A.sojae + ' ' + dbo.sf_sojae_nm4(a.sojae, a.brand,a.year, a.season) as sojae   ,
         A.item  + ' ' + dbo.sf_item_nm4(a.item, a.brand,a.year, a.season) as item   ,
         A.out_seq,   
         A.sample_cd,   
			A.make_type,
         b.make_type as make_type2,   
         A.patt_type,   
         A.orgmat_cd,
			b.orgmat_cd	as orgmat_cd2,   
         A.dsgn_emp,   
         dbo.sf_emp_nm(a.dsgn_emp)   as dsgn_emp_nm, 
         A.concept,   
         A.plan_yn,   
         A.country_cd,
			isnull(b.country_cd,'00') as country_cd2,   
         A.tag_price,   
         A.style_info,
			b.cancel_yn,
			b.pay_gubn,
			case when exists(select top 1 * from tb_12030_s where style = b.style and chno = b.chno and isnull(in_qty,0) <> 0) then 1 else 0 end prot,
		   (select out_seq from tb_12028_d (nolock) where style = b.style and chno = b.chno)	out_seq2,
			a.style_type,
			a.washing_yn,
			a.mat_use_way,
			a.mat_direction,
			(select max(isnull(note, dbo.sf_inter_nm('211',claim_gubn) )) from tb_23010_m (nolock) where style = b.style and chno = b.chno) 	claim_note,
			a.smat_info	as smat_info  ,
         A.style_info2          
    FROM tb_12020_m  A,   
         tb_12021_d  B 
   WHERE B.style  =* A.style 
     AND B.chno  = :as_chno
     AND (A.style = :as_style) 
" arguments=(("as_style", string),("as_chno", string)) )
