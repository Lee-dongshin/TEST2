 retrieve="  SELECT A.Style                             AS Style,   
         dbo.sf_inter_nm('001', A.Brand)     AS Brand,   
         A.Year                              AS Year,   
         dbo.sf_inter_nm('003', A.Season)    AS Season,   
         dbo.sf_sojae_nm3(A.sojae, substring(a.style,1,1), a.year)            AS sojae,   
         dbo.sf_item_nm3(A.item, substring(a.style,1,1), a.year)              AS item,   
         A.Out_Seq,   
         A.Sample_Cd,   
         dbo.sf_inter_nm('030', A.Make_Type) AS Make_Type,   
         A.Patt_Type,   
         A.Dsgn_Emp,   
         dbo.Sf_Emp_Nm(A.Dsgn_Emp)           AS Dsgn_Nm, 
         dbo.sf_inter_nm('122', A.Concept)   AS Concept,   
         A.Plan_Yn,   
         A.Style_info,   
         A.Dep_Fg,   
         A.Dep_Ymd,   
         A.Dep_Seq,   
         A.Mod_id, 
         A.Mod_Dt, 
         B.Chno,   
         B.Mat_Cd,   
         B.Mat_Nm,   
         B.Cust_Cd,   
         dbo.Sf_Cust_Nm(B.Cust_Cd, 'S') AS Cust_Nm, 
         B.Tag_Ymd,   
         B.Make_Price,   
         B.Etc_Price, 
         B.Remark   
    FROM TB_12020_M A,   
         TB_12021_D B 
   WHERE A.Style = B.Style    
     AND B.Style = :as_Style 
     AND B.Chno  = :as_Chno 
" update="TB_12020_M" updatewhere=0 updatekeyinplace=yes arguments=(("as_style", string),("as_chno", string)) )
