 column=(type=char(1) updatewhereclause=yes name=brand dbname="Brand" )
 retrieve="    SELECT empno, kname+'('+
	    (select top 1 inter_data1 from tb_91011_c (nolock)
			where inter_grp = '900' and inter_cd = a.jikchk) +') '+ dbo.sf_dept_nm(dept_code) as kname,
           Case Saup_Gubn 
                When '01' Then 'N' 
                When '02' Then 'O' 
					 When '11' Then 'W' 
                When '21' Then 'T' 
                ELSE '%' 
           End                              AS Brand 

      FROM MIS.DBO.THB01 a(nolock) --where empno = '970104'
      where goout_gubn = '1'
		and   dept_code not like '_A%'
	and (jikguk <> '00'

	or		exists (select * from mis.dbo.thb02 m(nolock)
			where m.dept_code = a.dept_code
			and   m.grp_cd1 = (select y.grp_cd1 from mis.dbo.thb01 x(nolock), mis.dbo.thb02 y(nolock)
						where x.dept_code = y.dept_code
						and   x.empno = :empno))				
		)
order by jikchk
" arguments=(("empno", string)) )
