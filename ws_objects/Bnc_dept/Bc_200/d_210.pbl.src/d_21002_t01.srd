 retrieve="  SELECT a.style,   
         a.test_date,
         a.test_detail,   
         a.reg_id,
			b.person_nm,
			a.reg_dt,
         convert(varchar,a.reg_dt,111) as reg_dt1,   
         a.mod_id,
			(select person_nm from tb_93010_m where person_id = a.mod_id) as mod_nm,   
         a.mod_dt,
			convert(varchar,a.mod_dt,111) as mod_dt1,
         '1' as flag
    FROM dbo.test_0226_m a left outer join tb_93010_m b on a.reg_id = b.person_id
	WHERE a.style like :is_style
" update="dbo.test_0226_m" updatewhere=0 updatekeyinplace=yes arguments=(("is_style", string))  sort="style A " )
