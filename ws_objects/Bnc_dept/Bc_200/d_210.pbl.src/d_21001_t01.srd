 retrieve="select dbo.sf_sojae_nm(a.mat_sojae) as mat_sojae_nm,
       a.mat_cd, 
	 	 a.mat_nm,
	 	 a.color +'  ' +  dbo.sf_color_nm(a.color, 'e') as  color_nm,
 	 	 a.spec, 
	 	 a.unit + '  ' + dbo.sf_inter_nm('004',  a.unit) as unit_nm, 
       a.req_price,          
   	 a.nego_price,       
       a.in_price,            
	 	 a.country_cd + '  ' +  dbo.sf_inter_nm('000', a.country_cd) as country_nm, 
       b.Ford_ymd,	    
	 	 b.Fin_ymd,                
	 	 b.Fout_ymd,
		 a.brand
  from vi_21010_1 a with (nolock),  
       tb_29012_s b with (nolock)  
 where a.brand      like :is_brand
	and a.mat_year   like :is_mat_year
	and a.mat_season like :is_mat_season
	and a.mat_sojae  like :is_sojae
	and a.mat_cd     like :is_mat_cd
	and a.mat_cd      *= b.mat_cd
	and a.color       *= b.color
" arguments=(("is_brand", string),("is_mat_year", string),("is_mat_season", string),("is_sojae", string),("is_mat_cd", string))  sort="brand A mat_sojae_nm A mat_cd A mat_nm A " )
