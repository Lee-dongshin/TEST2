 retrieve="  SELECT A.BRAND, A.IN_YMD, A.IN_GUBN, A.IN_NO,
         A.COLOR, dbo.sf_color_nm(A.COLOR, 'E') AS COLOR_NM,
         ISNULL(A.TMP_QTY, 0) AS TMP_QTY, 
         ISNULL(A.QC_QTY, ISNULL(A.TMP_QTY, 0)) AS QC_QTY,
         ISNULL(A.DFT_1, 0) AS DFT_1, 
         ISNULL(A.DFT_2, 0) AS DFT_2, 
         ISNULL(A.DFT_3, 0) AS DFT_3, 
         ISNULL(A.DFT_4, 0) AS DFT_4,
         ISNULL(A.DFT_5, 0) AS DFT_5, 
         ISNULL(A.DFT_6, 0) AS DFT_6, 
         ISNULL(A.DFT_7, 0) AS DFT_7, 
         ISNULL(A.DFT_8, 0) AS DFT_8, 
         ISNULL(A.DFT_9, 0) AS DFT_9, 
         ISNULL(A.DFT_QTY, 0) AS DFT_QTY,
         A.MOD_ID, A.MOD_DT, B.SPEC,
         (CASE ISNULL(B.SIL_QTY, 0) WHEN 0 THEN 'N' ELSE 'Y' END) AS SIL_CHK,
         C.DATA1, C.DATA2, C.DATA3, C.DATA4, C.DATA5, C.DATA6, C.DATA7, C.DATA8
    FROM TB_22011_D A,
         TB_22010_H B,
         ( SELECT CAST(MAX(CASE INTER_CD WHEN '1' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA1,
                  CAST(MAX(CASE INTER_CD WHEN '2' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA2,
                  CAST(MAX(CASE INTER_CD WHEN '3' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA3,
                  CAST(MAX(CASE INTER_CD WHEN '4' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA4,
                  CAST(MAX(CASE INTER_CD WHEN '5' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA5,
                  CAST(MAX(CASE INTER_CD WHEN '6' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA6,
                  CAST(MAX(CASE INTER_CD WHEN '7' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA7,
                  CAST(MAX(CASE INTER_CD WHEN '8' THEN INTER_DATA1 END) AS DECIMAL(8, 2)) AS DATA8
             FROM TB_91011_C
            WHERE INTER_GRP = '210'
         ) C
   WHERE A.BRAND   = B.BRAND
     AND A.IN_YMD  = B.IN_YMD
     AND A.IN_GUBN = B.IN_GUBN
     AND A.IN_NO   = B.IN_NO
     AND A.BRAND  = :as_brand
     AND A.IN_GUBN = '01'
     AND A.MAT_CD = :as_mmat_cd
" update="TB_22011_D" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_mmat_cd", string))  sort="in_ymd A in_no A " )
