 retrieve="  SELECT A.BRAND,		A.IN_YMD,		A.IN_GUBN,		A.IN_NO,
         A.IO_GUBN,	A.HOUSE,			A.CUST_CD,		
         A.MAT_CD,	dbo.sf_mat_nm(A.MAT_CD) AS MAT_NM,
         A.COLOR,		dbo.sf_color_nm(A.COLOR, 'E') AS COLOR_NM,
         A.SPEC,		A.UNIT,
         ISNULL(A.TMP_QTY,   0) AS TMP_QTY,
         ISNULL(A.SIL_QTY,   0) AS SIL_QTY,
         ISNULL(A.PRICE, 0) AS PRICE,
         ISNULL(A.AMT,   0) AS AMT,
         ISNULL(A.AMT, 0) - ISNULL(A.OUT_AMT, 0) AS GGONG_AMT,
         ISNULL(A.OUT_PRICE, 0) AS OUT_PRICE,
         ISNULL(A.OUT_AMT,   0) AS OUT_AMT,
         A.MAT_YEAR,	A.MAT_SEASON,	A.MAT_SOJAE,	A.MAT_CHNO,	A.OUT_SEQ,	A.MAT_GUBN,
         A.REG_ID,	A.REG_DT,		A.MOD_ID,		A.MOD_DT
    FROM TB_22010_H A
   WHERE A.BRAND    = :as_brand
     AND A.IN_YMD  = :as_yymmdd
     AND A.IN_GUBN = '03'
     AND A.IO_GUBN  = '+'
     AND A.HOUSE    = :as_house
     AND A.CUST_CD  = :as_cust_cd
     AND A.MAT_GUBN = '1'
" update="TB_22010_H" updatewhere=1 updatekeyinplace=yes arguments=(("as_brand", string),("as_yymmdd", string),("as_house", string),("as_cust_cd", string))  sort="in_no A " )
