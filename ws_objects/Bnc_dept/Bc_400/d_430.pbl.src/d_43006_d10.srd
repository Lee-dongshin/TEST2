 column=(type=decimal(0) updatewhereclause=yes name=stock_qty dbname="stock_qty" )
 retrieve="   select size, 
          sum(in_qty + iwol_qty - out_qty - etc_outqty + rtrn_qty)   as stock_qty
      from tb_44012_s 
     where shop_cd    = '010000'
       and shop_type  = '1'
       and style      = :as_style
       and chno       like :as_chno +'%'
       and color      like :as_color + '%'
		 and  year > left(convert(char(08), dateadd(year, -10, getdate()), 112),4)
   group by size;
" arguments=(("as_style", string),("as_chno", string),("as_color", string)) )
