 column=(type=char(37) updatewhereclause=yes name=shop_cd dbname="shop_cd" )
 column=(type=char(22) updatewhereclause=yes name=shop_type dbname="shop_type" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_qty dbname="sale_qty" )
 retrieve="select a.yymmdd, 
       a.shop_cd + ' ' + dbo.sf_shop_nm(a.shop_cd, 'a') as shop_cd, 
       a.shop_type + ' ' + dbo.sf_inter_nm('009', a.shop_type) as shop_type, 
       sum(sale_qty) as sale_qty
from dbo.tb_53010_h a with (nolock)
where a.shop_cd 	= :as_shop_cd
and   a.style 		= :as_style
and   a.color 		like :as_color
and   a.size 		= :as_size
and   a.brand 		= :as_brand
and   a.chno  like  :as_chno
group by a.yymmdd,a.shop_cd, a.shop_type

