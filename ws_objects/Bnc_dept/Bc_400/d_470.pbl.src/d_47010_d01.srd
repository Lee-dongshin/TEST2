 retrieve="
SELECT DB,YYMMDD,STYLE, CHNO,COLOR, SIZE, SHOP_CD, DBO.SF_SHOP_NM(SHOP_CD,'S') shop_nm, 
 ISNULL((SELECT SUM(B.QTY)
	FROM TB_45030_H B (nolock)
	WHERe b.gubn = 'R' 
	and A.YYMMDD = B.YYMMDD
	and   a.shop_cd = b.shop_cd
	AND   A.STYLE = B.STYLE
	AND   A.CHNO = B.CHNO
	AND   A.COLOR = B.COLOR
	AND   A.SIZE  = B.SIZE		),0) AS rqst_all,


   FR_SHOP_CD, DBO.SF_SHOP_NM(FR_SHOP_CD,'S') fr_shop_nm, COUNT(DB) cnt,  SUM(QTY) /COUNT(DB) rqst_qty, STOCK_QTY ,
	CASE WHEN SUM(QTY) > STOCK_QTY THEN STOCK_QTY ELSE SUM(QTY) END AS AVAIL_RQST,
 ISNULL((SELECT SUM(B.QTY)
	FROM TB_45050_H B
	WHERE A.YYMMDD = B.YYMMDD
	AND   A.STYLE = B.STYLE
	AND   A.CHNO = B.CHNO
	AND   A.COLOR = B.COLOR
	AND   A.SIZE  = B.SIZE
	AND   A.FR_SHOP_CD = B.FR_SHOP_CD
	AND   A.DB = B.DB	),0) AS Y_QTY
FROM tb_45051_h A
WHERE YYMMDD BETWEEN  :ls_fr_ymd AND :ls_to_ymd
and style like :ls_style + '%'
and style like :ls_brand + '%'
and fr_shop_cd like :ls_fr_shop_cd + '%'
and shop_cd like :ls_to_shop_cd + '%'

GROUP BY YYMMDD, SHOP_CD, FR_SHOP_CD, STYLE, CHNO,COLOR, SIZE,  STOCK_QTY , DB
ORDER BY YYMMDD, SHOP_CD,  STYLE, CHNO,COLOR, SIZE,FR_SHOP_CD,  STOCK_QTY , DB

