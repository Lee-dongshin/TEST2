 column=(type=char(20) updatewhereclause=yes name=order_name dbname="tb_45030_h.order_name" )
 retrieve="

select a.order_no, ord_ymd, order_name  
from tb_45030_h a (nolock)
where a.gubn = 'c'
and a.yymmdd between  :as_fr_ymd and :as_to_ymd
and not exists ( select x.*
					 from 
						(select distinct a.ord_ymd, a.db, a.rtrn_invoice_no
							from tb_45030_h a (nolock),
								  (select distinct ord_ymd, db_origin, rtrn_invoice_no, db
									from tb_45030_h (nolock)
									where gubn = 'c'
									and yymmdd between  :as_fr_ymd and :as_to_ymd ) b
							
							where a.gubn = 'b' 
							and   a.db = b.db_origin ) x
						where a.db_origin = x.db)
" arguments=(("as_fr_ymd", string),("as_to_ymd", string)) )
