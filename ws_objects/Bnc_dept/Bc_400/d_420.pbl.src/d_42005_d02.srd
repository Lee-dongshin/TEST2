 retrieve="select DISTINCT 
       shop_cd, dbo.sf_shop_nm(shop_cd, 'a') as shop_nm,
       yymmdd, out_no,  out_type, 'N' AS OUT_PROC,
       'N'  as out_print,
		 Rqst_Gubn,
		 Rqst_Date,
		 Rqst_chno,
       Work_NO, 
       SUM(ISNULL(QTY,0)) AS QTY, shop_type

from tb_42020_work
where left(shop_cd,1) = :as_brand
and   yymmdd    = :as_yymmdd
and   house_cd  = :as_house_cd	
and   out_no  like :as_out_no
and   shop_cd like :as_shop_cd
and   style   like '_____' + :as_style + '%'
and   chno    like :as_chno
AND   COLOR   LIKE :AS_COLOR
AND   SIZE    LIKE :AS_SIZE
AND   YEAR    LIKE :AS_YEAR
AND   SEASON  LIKE :AS_SEASON
AND   SOJAE	  LIKE :AS_SOJAE
AND	ITEM    LIKE :AS_ITEM
and   substring(convert(varchar(5), isnull(work_no,0) + 10000), 2, 4) like case when :ai_work_no = 0 then '%'
                                                                    else  substring(convert(varchar(5), :ai_work_no + 10000), 2, 4) + '%' end
and   rqst_gubn like :as_deal_fg
GROUP BY shop_cd,
       yymmdd, out_no, out_type,
		 Rqst_Gubn,
		 Rqst_Date,
		 Rqst_chno,
       Work_NO , shop_type


order by 1,3,4" arguments=(("as_brand", string),("as_yymmdd", string),("as_out_no", string),("as_shop_cd", string),("as_style", string),("as_chno", string),("as_color", string),("as_size", string),("as_deal_fg", string),("ai_work_no", number),("AS_YEAR", string),("AS_SEASON", string),("AS_SOJAE", string),("AS_ITEM", string),("as_house_cd", string)) )
