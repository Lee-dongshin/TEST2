 retrieve="
select a.out_ymd, a.deal_seq, 
			a.shop_cd, 
			dbo.sf_shop_nm(a.shop_cd,'s') as shop_nm, 
			a.style, 
			a.chno,
			a.color,
			dbo.sf_color_nm(a.color,'e') as color_nm,
			a.size,
			a.deal_qty,
	    a.out_ymd AS TO_OUT_YMD,	
	  	 a.deal_seq as to_deal_seq  , 
		 'N' AS PROC_YN
from tb_52031_h a (NOLOCK), tb_12020_m b (NOLOCK)
where a.out_ymd = :AS_OUT_YMD
and a.shop_cd like :AS_BRAND + '%'
and a.deal_seq = :AL_DEAL_SEQ
and a.style = b.style
and a.deal_qty >= :al_deal_qty
order by a.shop_cd, 
			a.style, 
			a.chno,
			a.color,
			a.size
" arguments=(("as_brand", string),("as_out_ymd", string),("al_deal_seq", number),("al_deal_qty", number)) )
