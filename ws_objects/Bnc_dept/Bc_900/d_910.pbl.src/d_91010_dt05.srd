 retrieve="  SELECT A.SHOP_CD,		A.ST_DT,			A.ED_DT,
			A.BRAND, 		A.SHOP_DIV,		A.SALE_EMP,		
			B.SALE_EMPNM,	B.JUMN_NO,		B.CUST_CD AS EMP_CUST,		
			C.CUST_NAME AS CUST_NM,			C.CUST_SNAME AS CUST_SNM, 
			A.COMM_FG,	A.PAY_WAY,		
			C.SAUP_NO,		C.UPTE,			C.JONGMOK,	C.BANK,
         A.REG_ID,		A.REG_DT,		A.MOD_ID,	A.MOD_DT
    FROM TB_91103_H A,
			TB_91102_M B,
			VI_91102_1 C
	WHERE A.SALE_EMP = B.SALE_EMP
	  AND B.CUST_CD  *= C.CUSTCODE
	  AND A.SHOP_CD  = :as_shop_cd
	  AND A.ED_DT = '99999999'
" update="TB_91103_H" updatewhere=0 updatekeyinplace=yes arguments=(("as_shop_cd", string)) )
