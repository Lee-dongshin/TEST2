 column=(type=number updatewhereclause=yes name=wonga dbname="wonga" )
 column=(type=number updatewhereclause=yes name=house_stock dbname="house_stock" )
 column=(type=number updatewhereclause=yes name=house_wonga dbname="house_wonga" )
 column=(type=number updatewhereclause=yes name=shop_stock dbname="shop_stock" )
 column=(type=number updatewhereclause=yes name=shop_wonga dbname="shop_wonga" )
 retrieve="select a.yyyy, a.brand,
dbo.sf_inter_sort_seq('001',a.brand) as brand_no ,
dbo.sf_inter_nm('001',a.brand) as brand_nm,
a.year,
a.season,
dbo.sf_inter_nm('003',a.season) as season_nm,
dbo.sf_inter_sort_seq('003',a.season) as season_no,

   a.style, a.chno, a.wonga,
	sum(a.house_stock) as house_stock,  sum(a.house_wonga) as house_wonga,
	sum(a.shop_stock) as shop_stock,  sum(a.shop_wonga) as shop_wonga
from 
	(select 
	left(rtrim(ltrim(yyyy)),4) as yyyy ,
	left(rtrim(ltrim(brand)),1) as brand ,
	left(rtrim(ltrim(year)),4) as year,
	left(rtrim(ltrim(season)),1) as season ,
	left(rtrim(ltrim(style)),8) as style,
	left(rtrim(ltrim(chno)),1) as chno,
	wonga,
	0 as house_stock,
	0 as house_wonga,
	shop_stock,
	wonga_amt  as shop_wonga
	from tb_closing_account_shop
	union all
	select 
	left(rtrim(ltrim(yyyy)),4) as yyyy ,
	left(rtrim(ltrim(brand)),1) as brand ,
	left(rtrim(ltrim(year)),4) as year,
	left(rtrim(ltrim(season)),1) as season ,
	left(rtrim(ltrim(style)),8) as style,
	left(rtrim(ltrim(chno)),1) as chno,
	wonga,
	house_stock,
	wonga_amt as house_wonga ,
	0 as shop_stock,
	0 as shop_wonga
	from tb_closing_account_house ) a
where a.brand  = :as_brand
  and a.yyyy  like :As_yyyy + '%'
  and a.year   like :As_year + '%'
  and a.season   like :As_season + '%'
group by yyyy, brand, year, season, style, chno,wonga
order by 1, 2, 4, 6, 8, 9
