 retrieve="  SELECT TB_56015_D.Shop_Cd,   
         TB_56015_D.Brand,   
         TB_56015_D.Year,   
         TB_56015_D.Season, 
			TB_56015_D.Shop_type, 
			TB_56015_D.Sale_type, 
         TB_56015_D.Item, 
         TB_56015_D.End_Ymd,   
         TB_56015_D.Start_Ymd,   
         TB_56015_D.Dc_Rate,
         TB_56015_D.Reg_id,   
         TB_56015_D.Mod_id,   
         TB_56015_D.Mod_Dt  
    FROM TB_56015_D with (nolock)
   WHERE shop_cd = :as_shop_cd 
     and brand   = :as_brand 
     and year    like :as_year + '%'
     and season  like :as_season + '%'
	  and isnull(item, '%')	like :as_item + '%'
     and start_ymd >= :as_fr_ymd
     and case when end_ymd >= :as_to_ymd then :as_fr_ymd else end_ymd end <= :as_to_ymd" update="TB_56015_D" updatewhere=0 updatekeyinplace=yes arguments=(("as_shop_cd", string),("as_brand", string),("as_year", string),("as_season", string),("as_item", string),("as_fr_ymd", string),("as_to_ymd", string)) )
