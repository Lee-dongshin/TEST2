 column=(type=decimal(0) updatewhereclause=yes name=tag_price dbname="tb_12020_m.tag_price" )
 column=(type=char(12) updatewhereclause=yes name=dep_fg dbname="dep_fg" )
 column=(type=decimal(0) update=yes updatewhereclause=yes name=resv_rate dbname="tb_12021_d.resv_rate" )
 column=(type=char(6) update=yes updatewhereclause=yes name=mod_id dbname="tb_12021_d.mod_id" )
 column=(type=datetime update=yes updatewhereclause=yes name=mod_dt dbname="tb_12021_d.mod_dt" )
 retrieve="  SELECT a.style + a.chno  as style_no, 
         a.style,   
         a.chno,   
         b.tag_price, 
         case b.dep_fg
            when 'Y' then '부진' 
            when 'O' then '시즌아웃부진'
            else '정상' 
         end   as dep_fg,   
         a.resv_rate, 
         a.mod_id,   
         a.mod_dt  
    FROM tb_12021_d a, 
         tb_12020_m b 
   where a.style = b.style 
     and b.brand  = :as_brand 
     and b.year   = :as_year 
     and b.season = :as_season 
     and b.sojae  like :as_sojae + '%' 
     and b.item   like :as_item  + '%' 

" update="tb_12021_d" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_year", string),("as_season", string),("as_sojae", string),("as_item", string)) )
