 retrieve="  select shop_cd, 
         shop_snm as shop_nm, 
         empno, 
         dbo.sf_emp_nm(empno)  as emp_nm, 
         deal_yn, 
         refill_yn,
			plan_deal_yn,
			area_cd, shop_div,
			refill_benifit,
			wan_yn,
			smart_rtrn,
		   rt_yn,
			mod_id,
			mod_dt,
			DIST_GRP,
			'%%'	DIST_EXAM,
			'01,03,04,05,06' AS INPUT_EXAM,
			md_note,
			edi_yn
    from tb_91100_m 
   where brand     =    :as_brand 
     and shop_div  LIKE :as_shop_div 
     and shop_stat = '00' 
     and shop_div  in  ('G', 'K', 'D','Z','E','B','X','T','M','S','O','I')
     and (shop_seq  < '5000' or shop_seq  = '5990')
   order by shop_div, area_cd, shop_cd 
" update="tb_91100_m" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_shop_div", string)) )
