table(column=(type=char(2) updatewhereclause=yes name=color dbname="color" )
 column=(type=char(2) updatewhereclause=yes name=size dbname="size" )
 column=(type=decimal(0) updatewhereclause=yes name=ord_qty dbname="ord_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=ord_deal dbname="ord_deal" )
 column=(type=decimal(0) updatewhereclause=yes name=ord_qty_chn dbname="ord_qty_chn" )
 column=(type=decimal(0) updatewhereclause=yes name=ord_deal_chn dbname="ord_deal_chn" )
 retrieve=" select b.color    as color, 
         b.size     as size, 
         case when :as_style  in ('NC5AJ973', 'NC5AJ911','NC5AL911','NC5AJ910','NC5AS910', 'NC5MB628','NC5MB962','NC5MB964','NC5MJ965')  then
			isnull(a.in_qty,0) - isnull(a.in_qty_chn,0)
			else
         a.ord_qty - isnull(a.ord_qty_chn,0)  
			end as ord_qty, 
         convert(decimal(8), (case when :as_style in ('NC5AJ973', 'NC5AJ911','NC5AL911','NC5AJ910','NC5AS910', 'NC5MB628','NC5MB962','NC5MB964','NC5MJ965')  then
										isnull(a.in_qty,0) - isnull(a.in_qty_chn,0)
										else
							         a.ord_qty - isnull(a.ord_qty_chn,0)  
										end) * c.resv_rate / 100 ) as ord_deal,
         isnull(a.ord_qty_chn,0)  as ord_qty_chn, 
         isnull(a.ord_qty_chn,0)  as ord_deal_chn
    from tb_12030_s a with (nolock), 
         tb_12024_d b with (nolock), 
         (select case when dbo.sf_style_type(:As_style) = 'F' then 70 else 100 - isnull(dd.resv_rate, ee.resv_rate) end as resv_rate
            from tb_12021_d dd with (nolock), 
                 (select isnull(convert(decimal(3), inter_data1), 0) as resv_rate
                    from tb_91011_c 
                    where inter_grp = '001' 
                     and inter_cd  = left(:as_style, 1)) ee
           where dd.style = :as_style 
             and dd.chno  = :as_chno) c  
   where a.style = b.style 
     and a.chno  = b.chno 
     and a.color = b.color 
     and a.size  = b.size 
     and b.style = :as_style 
     and b.chno  = :as_chno 
	  and  case when :as_style in ('NC5AJ973', 'NC5AJ911','NC5AL911','NC5AJ910','NC5AS910', 'NC5MB628','NC5MB962','NC5MB964','NC5MJ965')  then
			isnull(a.in_qty,0) - isnull(a.in_qty_chn,0)
			else
         a.ord_qty - isnull(a.ord_qty_chn,0)  
			end <> 0
   order by 1, 2" arguments=(("as_style", string),("as_chno", string)) )
