 retrieve="  Select isnull(B.YYMMDD, A.T_Date)  AS YYmmdd, 
         isnull(B.Brand,  :as_brand) AS Brand,  
         CASE A.Holiday_Yn 
              WHEN 'Y' THEN isnull(B.Auto_Yn, 'N') 
              ELSE isnull(B.Auto_Yn, 'Y') 
         END                         AS Auto_Yn, 
         B.Proc_Yn                   AS Proc_Yn, 
         B.Reg_id                    AS Reg_id, 
         B.Mod_id                    AS Mod_id, 
         B.Mod_Dt                    AS Mod_Dt, 
         A.Week_Cd                   AS Week_Cd, 
         A.Holiday_Yn                AS Holiday_Yn, 
         A.Remark                    AS Remark, 
         CASE B.Brand 
            WHEN NULL THEN 'NEW' 
            ELSE 'MOD' 
         END                         AS Row_Stat  
    From TB_DATE    A, 
         TB_52010_H B 
   Where A.T_DATE *= B.YYmmdd 
     And T_Date like :as_yymm + '%' 
     And B.Brand   = :as_brand  

" update="TB_52010_H" updatewhere=1 updatekeyinplace=no arguments=(("as_Brand", string),("as_yymm", string)) )
