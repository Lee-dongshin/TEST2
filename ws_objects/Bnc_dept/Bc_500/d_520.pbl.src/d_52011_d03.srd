 retrieve="  SELECT a.style,   
         a.chno,   
         a.shop_cd,   
         a.color,   
         a.size,   
         a.deal_qty,   
         a.out_ymd,   
         a.reg_id,   
         a.reg_dt,   
         a.mod_id,   
         a.mod_dt, 
         b.shop_snm  as shop_nm, 
         b.shop_stat as shop_stat, 
         b.deal_yn   as deal_yn   
    FROM tb_52030_m a, 
         tb_91100_m b 
   WHERE b.shop_cd = a.shop_cd 
     AND a.style   = :as_style 
     AND a.chno    = :as_chno 
     AND a.color   = :as_color 
   ORDER BY B.SHOP_DIV, B.AREA_CD, B.SHOP_CD 
" update="tb_52030_m" updatewhere=0 updatekeyinplace=yes arguments=(("as_style", string),("as_chno", string),("as_color", string)) )
