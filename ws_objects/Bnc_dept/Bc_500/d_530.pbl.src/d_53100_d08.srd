 retrieve="  SELECT shop_type,   
         sale_no,   
         no,   
         style + chno as style_no, 
         style,   
         chno,   
         color,   
         size,   
         sale_type,   
         sale_qty,   
         tag_price,   
         curr_price,   
         dc_rate,   
         sale_price,   
         tag_amt,   
         curr_amt,   
         sale_amt,   
         out_rate,   
         out_amt,   
         sale_rate,   
         sale_collect,   
         io_amt,   
         io_vat,   
         age_grp,   
         sale_fg,   
         jumin,   
         dbo.sf_member_nm('1', jumin)   as member_nm, 
         card_no, 
         goods_amt,
			coupon_no,
			shop_cd,
			dbo.sf_shop_nm(shop_cd, 'S') AS SHOP_NM,
			empty_1,
			empty_6,
			yymmdd,
			reg_dt,
			reg_id
    FROM tb_53017_h   
   WHERE yymmdd between :as_fr_yymmdd and :as_to_yymmdd 
     and shop_cd   like :as_shop_cd
     and shop_type like :as_shop_type 
     and brand     =    :as_brand 
     and style     like :as_style + '%'
	  and chno      like :as_chno + '%'
	  and empty_1 like :as_empty_gubn
   order by shop_cd, yymmdd, sale_no, no 

" arguments=(("as_brand", string),("as_fr_yymmdd", string),("as_to_yymmdd", string),("as_shop_cd", string),("as_shop_type", string),("as_style", string),("as_chno", string),("as_empty_gubn", string)) )
