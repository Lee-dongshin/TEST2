 retrieve="  SELECT yymmdd,
			shop_type,   
         sale_no,   
         no,   
         style + chno as style_no, 
         style,   
         chno,   
         color,   
         size,   
         sale_type,   
         sale_qty,   
         tag_price,   
         curr_price,   
         dc_rate,   
         sale_price,   
         tag_amt,   
         curr_amt,   
         sale_amt,   
         out_rate,   
         out_amt,   
         sale_rate,   
         sale_collect,   
         io_amt,   
         io_vat,   
         age_grp,   
         sale_fg,   
         jumin,   
         dbo.sf_member_nm('1', jumin)   as member_nm, 
         card_no, 
         goods_amt,
			coupon_no   
    FROM tb_53010_h   
   WHERE yymmdd    between  :as_yymmdd and :as_to_ymd
     and shop_cd   =    :as_shop_cd
     and shop_type like :as_shop_type 
     and brand     =    :as_brand 
   order by shop_type, sale_no, no 

" arguments=(("as_brand", string),("as_yymmdd", string),("as_to_ymd", string),("as_shop_cd", string),("as_shop_type", string))  sort="yymmdd A shop_type A " )
