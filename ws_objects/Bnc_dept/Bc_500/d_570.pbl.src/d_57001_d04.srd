 retrieve="  SELECT yymmdd,   
         send_id,   
         mes_seq,   
         recv_id,   
         dbo.sf_user_nm(recv_id) as shop_nm,   
         send_dt,   
         confirm_yn,   
         confirm_dt,   
         Reg_id,   
         Reg_Dt,   
         Mod_id,   
         Mod_Dt,
			(select  max(a.kname) from vi_93010_4 a where a.jumn_no = b.jumn_no and a.shop_cd not like '_H%' and a.shop_cd = b.recv_id)	as kname	
    FROM TB_57011_H b with (nolock)
   WHERE ( b.yymmdd = :as_yymmdd ) AND  
         ( b.send_id = :as_send_id ) AND  
         ( b.mes_seq = :ai_mes_seq )   
ORDER BY b.recv_id ASC   
" update="TB_57011_H" updatewhere=0 updatekeyinplace=yes arguments=(("as_yymmdd", string),("as_send_id", string),("ai_mes_seq", number))  sort="shop_nm A " )
