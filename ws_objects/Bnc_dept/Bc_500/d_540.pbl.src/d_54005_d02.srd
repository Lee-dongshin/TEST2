 retrieve="  SELECT a.style,   
         a.brand,   
         a.year,   
         a.season,   
         a.sojae,   
         a.item,   
         a.out_seq,   
         a.dep_seq,   
         a.dep_ymd, 
         a.rtrn_ymd,  
         a.dep_fg,   
         a.sel_yn,   
         a.rtrn_yn,   
         a.reg_id,   
         a.reg_dt,   
         a.mod_id,   
         a.mod_dt,
			a.dotcom  
    FROM tb_54020_h a 
   where a.brand = :as_brand
     and a.year  = :as_year
     and a.season = :as_season
     and a.sojae like :as_sojae
     and a.item  like :as_item
     and a.out_seq like :as_out_seq 
     and a.dep_fg = 'N'       
" update="tb_54020_h" updatewhere=0 updatekeyinplace=yes arguments=(("as_brand", string),("as_year", string),("as_season", string),("as_sojae", string),("as_item", string),("as_out_seq", string)) )
