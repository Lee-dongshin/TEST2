 column=(type=datetime updatewhereclause=yes name=mod_dt dbname="tb_54022_h.mod_dt" )
 column=(type=decimal(0) updatewhereclause=yes name=tag_price dbname="TAG_PRICE" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_price dbname="SALE_PRICE" )
 retrieve="  SELECT A.style,   
         A.brand,   
         A.year,   
         A.season,   
         A.sojae,   
         A.item,   
         A.out_seq,   
         A.dep_seq,   
         A.dep_ymd,   
         A.dep_fg,
         (case   A.dep_fg when 'Y'  then  '확정'
                        when 'n'  then  '미확정'
         end) as dep_name,
         A.sel_yn,   
         A.rtrn_yn,   
         A.reg_id,   
         A.reg_dt,   
         A.mod_id,   
         A.mod_dt, 
			DBO.SF_FIRST_PRICE(A.STYLE)		AS TAG_PRICE,
	
         isnull(
			 (SELECT min(b.sale_price) 		 
			  FROM tb_56012_d B (NOLOCK)			   
					  WHERE B.STYLE = A.STYLE 
				and  b.sale_price <> 0	
				 and  substring(b.shop_cd,2,1) =  'G'
				 and  convert(char(08), getdate(),112) between start_ymd and end_ymd
			  group by b.style	 )  ,
		
			 (SELECT dbo.sf_first_price(B.style) * (100 - max(b.dc_rate)) / 100
			  FROM tb_56012_d B (NOLOCK)			   
					  WHERE B.STYLE = A.STYLE 
				 and  substring(b.shop_cd,2,1) = 'G'
				 and  convert(char(08), getdate(),112) between start_ymd and end_ymd
			  group by b.style	 ) )  AS SALE_PRICE
    FROM tb_54022_h A (NOLOCK)  
   WHERE A.brand    =  :as_brand
     AND A.year     =  :as_year
     AND A.SEASON   =  :as_season
     AND A.SOJAE LIKE  :as_sojae + '%'
     AND A.ITEM  LIKE  :as_item + '%'
     AND A.DEP_SEQ LIKE :as_dep_seq + '%'   
     and A.DEP_FG  LIKE :as_dep_fg + '%'  
" update="tb_54022_h" updatewhere=0 updatekeyinplace=no arguments=(("as_brand", string),("as_year", string),("as_season", string),("as_sojae", string),("as_item", string),("as_dep_seq", string),("as_dep_fg", string))  sort="dep_seq A out_seq A style A " )
