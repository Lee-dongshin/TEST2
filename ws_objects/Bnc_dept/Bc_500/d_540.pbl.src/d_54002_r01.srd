 column=(type=decimal(2) updatewhereclause=yes name=mat_qty dbname="MAT_QTY" )
 column=(type=decimal(0) updatewhereclause=yes name=advice_qty dbname="ADVICE_QTY" )
 column=(type=char(8) updatewhereclause=yes name=re_in_ymd dbname="re_in_ymd" )
 column=(type=decimal(0) updatewhereclause=yes name=reorder_cost dbname="reorder_cost" )
 column=(type=char(8) updatewhereclause=yes name=end_ymd dbname="end_ymd" )
 retrieve="   SELECT A.STYLE, 
			 A.CHNO, 
			 A.COLOR,dbo.sf_color_nm(A.COLOR,'E') AS  COLOR_NM,
			 isnull(SUM(B.ORD_QTY),0) AS ORD_QTY, 
			 isnull(SUM(B.IN_QTY),0) + isnull(SUM(B.IWOL_QTY),0) AS IN_QTY, 
			 isnull(SUM(B.OUT_QTY),0) AS OUT_QTY, 
          isnull(SUM(B.ETC_QTY),0) AS ETC_QTY,
			 MAX(B.OUT_YMD) AS OUT_YMD, 
			 isnull(SUM(B.SALE_QTY),0) AS SALE_QTY, 
			 MAX(B.SALE_YMD) AS SALE_YMD,
			 DATEDIFF(DD,  MAX(B.SALE_YMD), GETDATE() ) AS   SALE_DAY,
          isnull(SUM(A.AVG_SALE_QTY),0) AS AVG_SALE_QTY,
          MAX(a.MAT_CD) AS MAT_CD, 
          dbo.sf_mat_nm(MAX(A.MAT_CD))   AS MAT_NM, 
          isnull(dbo.sf_mat_stock_qty(MAX(A.MAT_CD),MAX(A.MAT_COLOR)),0) AS MAT_QTY, 
          isnull(max(A.ADVICE_QTY),0) AS ADVICE_QTY  ,
			 max(a.re_in_ymd)		as re_in_ymd,
			 max(a.reorder_cost)	as reorder_cost,
			 max(a.end_ymd)		as end_ymd
    FROM TB_54010_d A,   
         TB_12030_S B,   
         TB_12020_M D (NOLOCK)  
   WHERE ( A.BRAND = :as_brand ) AND  
         ( A.YEAR = :as_year ) AND  
         ( A.SEASON = :as_season ) AND   
         ( A.CHK_BIT = 'Y' ) AND  
         ( A.STYLE = D.STYLE ) AND  
         ( D.OUT_SEQ LIKE :as_out_seq ) AND  
	      ( CONVERT(CHAR(8) , A.REG_DT,112)  LIKE  :as_yymmdd + '%') AND
         ( A.STYLE = B.STYLE ) AND  
         ( A.CHNO = B.CHNO ) AND  
         ( A.COLOR = B.COLOR )
    GROUP BY A.STYLE, A.CHNO, A.COLOR 
    ORDER BY A.STYLE, A.CHNO, A.COLOR    
" arguments=(("AS_BRAND", string),("AS_YEAR", string),("AS_SEASON", string),("AS_OUT_SEQ", string),("AS_YYMMDD", string)) )
