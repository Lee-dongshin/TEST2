table(column=(type=char(10) updatewhereclause=yes name=style dbname="style" )
 retrieve="
select distinct b.style+'-'+b.chno  as style
from tb_12021_d b
where b.mat_cd in (select distinct mat_cd 
			From tb_12025_d
			where style = :as_style
			and   chno like  :as_chno
			and   substring(mat_cd, 2,1) = '1'  )
and   b.style <> :as_style
union
select distinct b.style+'-'+b.chno from tb_12021_d b
where b.style like substring(:as_style, 1,4) + '_' + substring(:as_style, 6,8)
and   b.style <> :as_style" arguments=(("as_style", string),("as_chno", string)) )
