 retrieve="  SELECT yymmdd,   
         shop_cd,   
         shop_type,   
         sale_no,   
         no,   
         style + chno as style_no, 
         style,   
         chno,   
         color,   
         size,   
         sale_type,   
         sale_qty,   
         tag_price,   
         curr_price,   
         dc_rate,   
         sale_price,   
         tag_amt,   
         curr_amt,   
         sale_amt,   
         out_rate,   
         out_amt,   
         sale_rate,   
         sale_collect,   
         io_amt,   
         io_vat,   
         age_grp,   
         sale_fg,   
         jumin,   
         card_no, 
         right(card_no, 9) as card_no2,   
         goods_amt,   
         sale_cust,   
         brand,   
         year,   
         season,   
         sojae,   
         item,   
         shop_div,   
         reg_id,   
         reg_dt,   
         mod_id,   
         mod_dt, 
         out_amt      / sale_qty as out_price, 
         sale_collect / sale_qty as collect_price,
			coupon_no,
			local_price   
    FROM tb_53013_h   
   WHERE yymmdd    = :as_yymmdd 
     AND shop_cd   = :as_shop_cd
     AND shop_type = :as_shop_type 
     AND sale_no   = :as_sale_no  
	  and no     like :as_no" update="tb_53013_h" updatewhere=0 updatekeyinplace=yes arguments=(("as_yymmdd", string),("as_shop_cd", string),("as_shop_type", string),("as_sale_no", string),("as_no", string)) )
