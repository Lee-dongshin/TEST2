 column=(type=decimal(0) updatewhereclause=yes name=in_qty dbname="in_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_qty dbname="sale_qty" )
 retrieve="
select a.size,
		 case when :As_chn_gubn = 'K' then  case when sum(isnull(a.in_qty,0)) = 0 then  sum(isnull(a.iwol_qty,0)) - sum(isnull(a.in_qty_chn,0)) 
															  else   sum(isnull(a.in_qty,0)) - sum(isnull(a.in_qty_chn,0)) end
				when :As_chn_gubn = 'C' then  sum(isnull(a.in_qty_chn,0))
				when :As_chn_gubn = 'R' then  sum(isnull(a.in_qty_rus,0))
				else case when sum(isnull(a.in_qty,0)) = 0 then sum(isnull(a.iwol_qty,0)) else sum(isnull(a.in_qty,0)) end end as  in_qty,
		 case when :As_chn_gubn = 'K' then   case when :as_sale_gubn = '0' then sum(a.sale_qty) - ( sum(a.sale_qty_9) - sum(a.sale_qty_chn) - sum(a.sale_qty_rus))
																when :as_sale_gubn = '1' then sum(a.sale_qty_1) 
																else sum(a.sale_qty) - sum(a.sale_qty_chn) - sum(a.sale_qty_rus) end 
				when :As_chn_gubn = 'C' then   sum(a.sale_qty_chn) 
				when :As_chn_gubn = 'R' then   sum(a.sale_qty_rus) 
				else   case when :as_sale_gubn = '0' then sum(a.sale_qty) - sum(a.sale_qty_9) 
								when :as_sale_gubn = '1' then sum(a.sale_qty_1) 
								else sum(a.sale_qty) end  
		end as sale_qty
from tb_12030_s a (nolock) left outer join VI_12020_1 b on a.style = b.style and a.chno = b.chno
where a.style    = :as_style
and   a.chno  like :as_chno
AND b.dep_fg   not in (:as_dep_fg)

group by a.size" arguments=(("as_style", string),("as_chno", string),("as_chn_gubn", string),("as_sale_gubn", string),("as_dep_fg", string)) )
