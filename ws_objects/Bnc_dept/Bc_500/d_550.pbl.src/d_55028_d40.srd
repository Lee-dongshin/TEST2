table(column=(type=char(8) updatewhereclause=yes name=style dbname="style" )
 column=(type=char(1) updatewhereclause=yes name=chno dbname="chno" )
 column=(type=decimal(1) updatewhereclause=yes name=cost_price dbname="COST_PRICE" )
 column=(type=decimal(0) updatewhereclause=yes name=tag_price dbname="TAG_PRICE" )
 column=(type=char(2) updatewhereclause=yes name=size dbname="size" )
 column=(type=decimal(0) updatewhereclause=yes name=in_qty_size dbname="in_qty_size" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_qty_size dbname="sale_qty_size" )
 column=(type=char(8) updatewhereclause=yes name=out_ymd dbname="out_ymd" )
 column=(type=char(2) updatewhereclause=yes name=color dbname="color" )
 column=(type=decimal(0) updatewhereclause=yes name=in_qty_color dbname="in_qty_color" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_qty_color dbname="sale_qty_color" )
 retrieve="
select a.style,  a.chno, a.COST_PRICE, a.TAG_PRICE, b.size, b.in_qty	as in_qty_size, b.sale_qty as sale_qty_size,
	c.out_ymd, c.color,c.in_qty as in_qty_color, c.sale_qty as sale_qty_color                                

from 
(select distinct 	a.style, 
	case when :As_chno_gubn = 'C' then a.chno
		  else '%' end as chno,
	:As_chn_gubn	as chn_gubn	,
	:As_sale_gubn	As sale_gubn,
	case when :As_chno_gubn = 'C' then DBO.SF_GET_WONGA(a.style, A.CHNO) * 1.1
		  else  DBO.SF_GET_WONGA_AVG(a.style) * 1.1 end as COST_PRICE,
	DBO.SF_FIRST_PRICE(A.STYLE) AS TAG_PRICE
from tb_12030_s  a (nolock)
where a.BRAND  =    :As_brand
AND  a.YEAR    like    :as_year
AND a.SEASON   like    :as_season
and a.style    like :as_style + '%'
and a.sojae    like :as_sojae + '%'
and a.item    like :as_item + '%'
and a.item   <> case when :as_plan_gubn = 'Y' then '0' else 'Z' end ) a,


(select  style, 
		case when :As_chno_gubn = 'C' then chno
		  else '%' end as chno, size,
	 case when :As_chn_gubn = 'K' then  sum(isnull(in_qty,0)) - sum(isnull(in_qty_chn,0))
			when :as_chn_gubn = 'C' then  sum(isnull(in_qty_chn,0))
			else  sum(isnull(in_qty,0)) end as  in_qty,

	 case when :As_chn_gubn = 'K' then   
			case when :as_sale_gubn = '0' then sum(sale_qty) - ( sum(sale_qty_9) - sum(sale_qty_chn))
			     when :as_sale_gubn = '1' then sum(sale_qty_1) 
			else sum(sale_qty) - sum(sale_qty_chn) end 
	      when :As_chn_gubn = 'C' then   sum(sale_qty_chn) 
	else            case when :as_sale_gubn = '0' then sum(sale_qty) - sum(sale_qty_9) 
			     when :as_sale_gubn = '1' then sum(sale_qty_1) 
			else sum(sale_qty) end  
	end as sale_qty
from tb_12030_s (nolock)
where BRAND  =    :As_brand
AND  YEAR    like    :As_year
AND SEASON   like    :as_season
and style    like :as_style + '%'
and sojae    like :as_sojae + '%'
and item    like :as_item + '%'
group by style, case when :As_chno_gubn = 'C' then chno
		  else '%' end, size) b,


(select right(min(out_ymd),4)	as out_ymd,
		style, 
		case when :As_chno_gubn = 'C' then chno
		  else '%' end as chno, size,
		 color,	
		 case when :As_chn_gubn = 'K' then  sum(isnull(in_qty,0)) - sum(isnull(in_qty_chn,0))
				when :As_chn_gubn = 'C' then  sum(isnull(in_qty_chn,0))
				else  sum(isnull(in_qty,0)) end as  in_qty,

		 case when :As_chn_gubn = 'K' then   case when :as_sale_gubn = '0' then sum(sale_qty) - ( sum(sale_qty_9) - sum(sale_qty_chn))
																when :as_sale_gubn = '1' then sum(sale_qty_1) 
																else sum(sale_qty) - sum(sale_qty_chn) end 
				when :As_chn_gubn = 'C' then   sum(sale_qty_chn) 
				else   case when :as_sale_gubn = '0' then sum(sale_qty) - sum(sale_qty_9) 
								when :as_sale_gubn = '1' then sum(sale_qty_1) 
								else sum(sale_qty) end  
		end as sale_qty
from tb_12030_s (nolock)
where BRAND  =    :As_brand
AND  YEAR    like    :As_year
AND SEASON   like    :as_season
and style    like :as_style + '%'
and sojae    like :as_sojae + '%'
and item    like :as_item + '%'
group by style, case when :As_chno_gubn = 'C' then chno
		  else '%' end, size, color) c 
where a.style = b.style
and   a.chno = b.chno
and   b.style = c.style
and   b.chno  = c.chno
and   b.size  = c.size
order by a.style, b.size, c.color" arguments=(("as_brand", string),("as_year", string),("as_season", string),("as_sojae", string),("as_item", string),("as_chno_gubn", string),("as_plan_gubn", string),("as_chn_gubn", string),("as_sale_gubn", string),("as_style", string)) )
