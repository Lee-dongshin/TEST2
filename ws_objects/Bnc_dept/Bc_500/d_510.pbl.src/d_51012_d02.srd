 retrieve="SELECT 
SHOP_CD,
DBO.SF_SHOP_NM(SHOP_CD, 'S')	AS SHOP_NM,
EMP_CLASS,
EMP_NM,
BIRTH_DAY,
TEL_NO,
BEGIN_YMD,
END_YMD,
GOOUT_GUBN,
BRAND,
SHOP_DIV,
REG_ID,
REG_DT,
MOD_ID,
MOD_DT,
jumin,
empno,
address,
hobong,
dbo.sf_pic_emp(jumin)	as pic_emp,
(select dbo.sf_emp_nm(empno) from tb_91100_m (nolock)
	where shop_cd = a.shop_cd) as person_nm,
dbo.sf_emp_nm(isnull(a.mod_id,a.reg_id)) as mod_nm,
isnull(a.mod_dt,a.reg_dt) as mod_dt,
bank_code, tong_no, 
dept_cd,
apply_yn
FROM TB_51040_H a(NOLOCK)
WHERE SHOP_CD    = :AS_SHOP_CD
AND   EMP_CLASS  = :AS_EMP_CLASS
AND   EMP_NM	  = :AS_EMP_NM
AND   GOOUT_GUBN LIKE :AS_GOOUT_GUBN" update="TB_51040_H" updatewhere=1 updatekeyinplace=yes arguments=(("AS_SHOP_CD", string),("AS_EMP_CLASS", string),("AS_EMP_NM", string),("AS_GOOUT_GUBN", string)) )
