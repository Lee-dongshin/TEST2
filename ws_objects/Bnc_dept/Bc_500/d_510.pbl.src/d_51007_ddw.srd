table(column=(type=char(6) updatewhereclause=yes name=person_id dbname="person_id" )
 column=(type=char(20) updatewhereclause=yes name=person_nm dbname="person_nm" )
 retrieve="	select distinct a.empno 		as person_id, 
			dbo.sf_emp_nm(a.empno) as person_nm
	from tb_91100_m a (nolock) , mis.dbo.thb01 b (nolock)
	where a.brand like :brand + '%'
	and   a.shop_stat = '00' 
   and   a.empno = b.empno
   and   b.goout_gubn = '1'
	and   ( a.empno is not null and len(rtrim(a.empno)) <> 0)
	and   a.shop_div in ('g','k','x','H','B','E','o', case when :brand in ('B','S','E','G') then 'E' else 'g' end, case when :brand in ('B','S','E','G') then 'D' else 'g' end, case when :brand in ('B','S','E','G') then 'T' else 'g' end)

	union all

	select person_id, person_nm
	from tb_93010_m 
	where brand = :brand
			and person_id like 'K[0-9]%'" arguments=(("brand", string)) )
