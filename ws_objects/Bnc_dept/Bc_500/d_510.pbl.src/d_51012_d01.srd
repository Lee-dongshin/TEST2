 retrieve="
SELECT 
	EMP_CLASS,
	DBO.SF_INTER_NM('510',EMP_CLASS)			AS CLASS_NM,
	DBO.SF_INTER_SORT_SEQ('510',EMP_CLASS) 	AS SORT_SEQ,
	EMP_NM,
	shop_cd, dbo.sf_shop_nm(shop_cd,'s')	as shop_nm,
   case when isnull(apply_yn,'N') = 'Y' then 0 else 1 end as flag
FROM TB_51040_H (NOLOCK)
WHERE SHOP_CD like :AS_SHOP_CD + '%'
AND   GOOUT_GUBN LIKE :AS_GOOUT_GUBN + '%'
and   emp_nm  like :as_emp_nm + '%'
ORDER BY isnull(mod_dt,reg_dt) desc
" arguments=(("AS_SHOP_CD", string),("AS_GOOUT_GUBN", string),("as_emp_nm", string))  sort="flag D " )
