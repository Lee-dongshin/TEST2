 retrieve="
select 	a.yymmdd, a.out_no, a.no,
			a.shop_cd, dbo.sf_shop_nm(a.shop_cd, 's')	as shop_nm,
			a.out_type, a.house_cd,
			a.style, a.chno ,a.color , dbo.sf_color_nm(a.color, 'e') as color_nm,
			a.size,  b.tag_price, 	
			a.qty	as qty, b.item, b.sojae, b.year, b.season, 
			a.pda_no, a.tran_ord
from TB_42027_H a (nolock), tb_12020_m b (nolock)
where a.yymmdd between :as_fr_ymd and :as_to_ymd	
  and a.shop_cd = :as_brand + 'D1900'
  and a.house_cd = '010000'
  and a.out_type = case when :as_outin_gubn = 'A' then 'O' else '1' end
  and a.year   like :as_year + '%'
  and a.season   like :As_season + '%'
  and a.style = b.style
order by a.yymmdd, a.out_no, a.no,b.year, b.season, a.style, a.chno ,a.color,a.size  " arguments=(("as_brand", string),("as_fr_ymd", string),("as_to_ymd", string),("as_outin_gubn", string),("as_year", string),("as_season", string)) )
