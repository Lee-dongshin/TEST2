 retrieve="select give_date, coupon_no, give_point * 10	as give_amt, give_rate, use_ymd, verify_no, '               ' as chk_verify , use_shop
from	 tb_71011_h (nolock)
where jumin       = :as_jumin
and	point_flag  = '1'
and   accept_flag = 'N'
and  (coupon_no in ('first1','second')
or    coupon_no like :as_brand+'%'
or    coupon_no like 'T%') 
and   isnull(use_shop,'%') = case when use_shop is null then '%' else :as_shop_cd end
and   isnull(use_ymd,'9') >= convert(char(8),getdate(),112) 
and   give_date <= convert(char(8),getdate(),112)  
order by give_date desc" arguments=(("as_jumin", string),("as_brand", string),("as_shop_cd", string)) )
