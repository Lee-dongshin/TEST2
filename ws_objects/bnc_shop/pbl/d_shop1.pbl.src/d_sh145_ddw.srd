 retrieve="
select shop_cd, shop_snm 
from tb_91100_m (nolock) 
where shop_div  = 'O' 
		and shop_stat = '00'
		and right(shop_cd,4) not in ('4251')
and brand = :brand

union all

select shop_cd, shop_snm 
from tb_91100_m (nolock) 
where shop_div  in ('H') 
		and shop_stat = '00'
		and brand = :brand
		and right(shop_cd,4) = right(:gs_shop_cd,4)

union all
select shop_cd, shop_snm 
from tb_91100_m (nolock) 
where right(shop_cd,5)  in ('x2710')
and brand = :brand

" update="tb_91100_m" updatewhere=1 updatekeyinplace=no arguments=(("brand", string),("gs_shop_cd", string)) )
