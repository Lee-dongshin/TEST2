 column=(type=decimal(0) updatewhereclause=yes name=qty dbname="qty" )
 column=(type=decimal(0) updatewhereclause=yes name=out_qty dbname="out_qty" )
 column=(type=decimal(0) updatewhereclause=yes name=sale_amt dbname="sale_amt" )
 column=(type=char(8) updatewhereclause=yes name=out_ymd dbname="tb_53060_h.out_ymd" )
 retrieve="
select 	kname, style + chno + color + size as style_no,
	sum(qty) 	as qty, 
	sum(out_qty)	as out_qty, 
	sum(sale_amt) as sale_amt,
	out_ymd			
from tb_53060_h (nolock)
where datepart(week, yymmdd) = datepart(week, getdate()) 
and  out_ymd like convert(char(04), getdate(), 112) + '%'
and empno in ( select empno from VI_93010_3
					where dept_code = :As_empno
					  and status_yn = 'y')
group by kname, style, chno, color, size, out_ymd" arguments=(("as_empno", string)) )
