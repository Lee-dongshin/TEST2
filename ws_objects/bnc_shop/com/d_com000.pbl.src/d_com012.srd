 retrieve="  SELECT STYLE + CHNO + COLOR + SIZE AS STYLE_NO, 
         STYLE, CHNO, 
         COLOR, 
         dbo.sf_color_nm(COLOR, 'E')    AS color_nm, 
         SIZE, 
         BRAND, 
         SOJAE, 
         case	when brand in ('B','P')  and year <= '2013' then dbo.sf_sojae_nm(sojae)
					else dbo.sf_sojae_nm2(sojae, brand) end 			as SOJAE_NM,
         YEAR, 
         SEASON, 
         dbo.sf_inter_nm('003', SEASON) AS SEASON_NM, 
         ITEM, 
         case	when brand in ('B','P')  and year <= '2013' then dbo.sf_item_nm(item)
					else dbo.sf_item_nm2(item, brand) end            AS ITEM_NM, 
         CUST_CD,
         dbo.sf_cust_nm(CUST_CD, 'S')   AS CUST_NM,
         TAG_PRICE,
			PLAN_YN,
			dep_fg,
			dep_ymd,
			for_lotte,
			brand_grp
	 FROM VI_12024_shop  with (nolock)    
" )
